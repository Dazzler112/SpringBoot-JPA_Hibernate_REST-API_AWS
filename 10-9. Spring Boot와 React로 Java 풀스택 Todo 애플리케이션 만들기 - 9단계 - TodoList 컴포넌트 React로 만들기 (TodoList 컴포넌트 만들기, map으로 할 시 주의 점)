function ListTodosComponent() {

    //4: ì—¬ê¸°ì—ì„œ ë°°ì—´ì„ ë§Œë“¤ ê²ƒì´ë‹¤, {}ì´ê±´ í•œ ê°œ ìš”ì†Œë¥¼ ê°€ì§„ ë°°ì—´ì´ê³  ìš”ì†Œë¥¼ ë” ë§Œë“¤ì–´ ë³´ê² ë‹¤
    //ì—¬ê¸°ì— ìš”ì†Œë¥¼ ë‹´ëŠ” ê²ƒì´ë‹¤ ì²˜ìŒê»€ 1, Learn AWSì§€ ë‘ ë²ˆì§¸ëŠ” ì§€ê¸ˆ í•˜ê³  ìˆëŠ” ê±¸ ì ê² ë‹¤, Learn Full Stack Dev, ì„¸ ë²ˆì§¸ëŠ” Learn DevOpsë¼ê³  í•˜ê² ë‹¤ Kubernetesë‚˜ Dockerë„ ë°°ìš°ê³  ì‹¶ë‹¤ë©´ DevOpsë¥¼ ë°°ì›Œì•¼ê² ì§€
    //ê·¸ë¦¬ê³  ì—¬ê¸°ì— ë‹¤ ë³´ì—¬ì£¼ê³  ì‹¶ë‹¤, ì—¬ê¸° ìˆëŠ” ì„¸ ê°œì˜ ê°’ì„ ê°ê°ì˜ í–‰ìœ¼ë¡œ ì—¬ê¸° ë³´ì—¬ì£¼ê³  ì‹¶ì€ ê±°ì§€, ê·¸ ì „ì— ì¼ë‹¨ ì—¬ê¸°ë¡œ ê°€ì„œ JavaScript ì½˜ì†”ë¡œ ê°ˆ ê²ƒì´ë‹¤
    const todos = [
                    {id : 1, description : 'Learn AWS'},
                    {id : 1, description : 'Learn Full Stack Dev'},
                    {id : 1, description : 'Learn DevOps'}
                  ]

    return (
        <div className='ListTodosComponent'>
            <h1>Things You Want To Do!</h1>
            <div>
                <table>
                    <thead>
                        <tr>
                            <td>id</td>
                            <td>description</td>
                        </tr>
                    </thead>
                    <tbody>
                         <tr>
                         <td>{todos.id}</td>
                         <td>{todos.description}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    )
}
