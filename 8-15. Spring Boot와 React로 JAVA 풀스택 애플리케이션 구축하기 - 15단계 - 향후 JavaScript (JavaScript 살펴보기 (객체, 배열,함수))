вЏє VSC


вЏє LearningJavaScript.jsx



const person = {
    name: 'Ranga Karanam',
    address: {
        line1: 'Baker Street',
        city: 'London',
        country: 'UK',
    },
    profiles: ['twitter', 'linkedin', 'instagram'],
    printProfile: () => {
        person.profiles.map(
            //13: м—¬кё° кґ„нёлҐј м§Ђмљ°кі  profileмќґлќј н•њ л‹¤мќЊ м¤‘кґ„нёлЏ„ м§Ђмљ°л©ґ н›Ём”¬ лЌ” к°„л‹Ён•ґмЎЊм§Ђ, 
            profile => console.log(profile)
        ) 
    }
       //мЅ”л“њ м •л¦¬: person.profiles.map м—ђм„њ к°™мќЂ м¤„ н™”м‚ґн‘њ н•Ём€лҐј м •мќн–€л‹¤ profile console.log(profile)
}


export default function LearningJavaScript () {
    return(
        <>
        <div>{person.name}</div>
        <div>{person.address.line1}</div>
        <div>{person.profiles[0]}</div>
        <div>{person.printProfile()}</div>
        </>
    )
}

