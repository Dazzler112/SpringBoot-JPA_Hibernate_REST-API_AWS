const person = {
    name: 'Ranga Karanam',
    address: {
        line1: 'Baker Street',
        city: 'London',
        country: 'UK',
    },
    profiles: ['twitter', 'linkedin', 'instagram']

    //8: м—¬кё°м—ђ printProfileмќґлќјлЉ” н•Ём€лҐј л§Њл“¤кІ л‹¤, printProfile н•Ём€к°Ђ мќґлџ° к±ё н•њл‹¤кі  н•ґліґмћђ//8-1: м•„лћм—ђм„њ мћ‘м„±.. 
    printProfile:
}

рџџў мќјл°н•Ём€
//8-1: function printProfile, function printProfile мќґл ‡кІЊ ліґн†µ н•Ём€лҐј м •мќн•њл‹¤,
function printProfile () {
  //8-2: console.logлЎњ person.profilesмќ 0лІ€м§ёлҐј ліґкІ л‹¤, ліґн†µ мќґлџ° м‹ќмњјлЎњ н•њл‹¤ 
  console.log(person.profiles[0])
}

export default function LearningJavaScript () {
    return(
        <>
        <div>{person.name}</div>
        <div>{person.address.line1}</div>
        <div>{person.profiles[0]}</div>
        </>
    )
}
