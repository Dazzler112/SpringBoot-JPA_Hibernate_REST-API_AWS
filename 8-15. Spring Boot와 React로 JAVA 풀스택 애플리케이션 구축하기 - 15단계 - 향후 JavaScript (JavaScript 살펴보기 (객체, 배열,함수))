const person = {
    name: 'Ranga Karanam',
    address: {
        line1: 'Baker Street',
        city: 'London',
        country: 'UK',
    },
    //7: Rangaм—ђкІЊлЉ” м—¬лџ¬ н”„лЎњн•„мќґ мћ€л‹¤кі  н•кІ л‹¤, лЄЁл“  н”„лЎњн•„мќ„ л‚нѓЂл‚ґлЉ” л°°м—ґмќґ мћ€л‹¤, Twitter н”„лЎњн•„мќґ мћ€мќ„к±°кі , LinkedIn н”„лЎњн•„, instagram н”„лЎњн•„лЏ„ мћ€мќ„ м€ мћ€кІ м§Ђ
    //мќґкІЊ л°°м—ґмќґл‹¤, мќґкІЊ JavaScriptм—ђм„њ л°°м—ґмќ„ л‚нѓЂл‚ґлЉ” л°©лІ•мќґл‹¤, л°°м—ґм—ђ м ‘к·јн•лЉ” л°©лІ•мќЂ Javaм™Ђ л§¤мљ° л№„мЉ·н•л‹¤//7-1: LearningJavaScriptм»ґнЏ¬л„ЊнЉёлЎњ..
    profiles: ['twitter', 'linkedin', 'instagram']
}


export default function LearningJavaScript () {
    return(
        <>
        <div>{person.name}</div>
        <div>{person.address.line1}</div>
        
        //7-1: мІ« лІ€м§ё мљ”м†Њм—ђ м ‘к·јн•кі  м‹¶мњјл©ґ divлќј н•кі  profiles[0]мќґлќјкі  н•кІ л‹¤ г…‡г…‹ profilesмќ 0лІ€м§ёлЉ” л­к№Њ? м‚¬м‹¤ profilesмќ 0лІ€м§ёк°Ђ м•„л‹€лќј
        //person.profilesмќ 0лІ€м§ём—¬м•ј н•њл‹¤, к·ёл¦¬кі  к·ё к°’м—ђ м ‘к·јн•кі  м‹¶м§Ђ м¤‘кґ„нё м•€м—ђ л„Јм–ґм¤м•ј н•њл‹¤, к·ёл¦¬кі  н™•мќён•ґліґмћђ
        <div>{person.profiles[0]}</div>       
        </>
    )
}

