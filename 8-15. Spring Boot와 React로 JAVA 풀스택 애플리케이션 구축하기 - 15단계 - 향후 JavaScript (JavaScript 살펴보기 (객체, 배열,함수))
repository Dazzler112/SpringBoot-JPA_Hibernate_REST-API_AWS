const person = {
    name: 'Ranga Karanam',

    //6: м—¬кё°м—ђ мќґл ‡кІЊ к°ќмІґлҐј мѓќм„±н•  м€ мћ€к±°л“ , мќјл‹Ё мќґк±ё м§Ђмљ°кі  ('123 Baker Street London' м‚­м њн›„ {}мћ‘м„±) м¤‘кґ„нёлҐј м—ґкі  л‹«кі 
    address: {
      //6-1: м—¬кё° м•€м—ђл‹¤ line1мќ„ м“°кі  Baker Street
      line1: 'Baker Street',
      //6-1кі„м†Ќ: cityлЉ” Londonмќґкі 
      city: 'London',
      //6-1кі„м†Ќ: м›ђн•л©ґ countryлЉ” UKлќј м¤„ м€ мћ€кІ м§Ђ, к°Ѓ м†Ќм„±мќЂ мЅ¤л§€лЎњ кµ¬л¶„н•њл‹¤
      //мЅ¤л§€, мЅ¤л§€ к·ёл¦¬кі  л§€м§Ђл§‰ м†Ќм„± л‹¤мќЊм—ђлЏ„ мЅ¤л§€лҐј л¶™мќј м€ мћ€л‹¤, м—¬кё°м—ђлЏ„ л¶™мќј м€ мћ€кі 
      country: 'UK',
    },//JavaScriptлЉ” л­ђлќјкі  н•м§Ђ м•ЉлЉ”л‹¤, м ЂмћҐн•л©ґ лђњл‹¤ //6-2: LearningJavaScriptм»ґнЏ¬л„ЊнЉёлЎњ..
}


export default function LearningJavaScript () {
    return(
        <>
        <div>{person.name}</div>

        //6-2: person.address.line1мќґлќјкі  н•кі  л‹¤лҐё кІѓлЏ„ м¶”к°Ђн•  м€ мћ€кІ м§Ђ
        //м¤„л§€л‹¤ city, countryл“± л‹¤лҐё м†Ќм„±мќ„ м¶”к°Ђн•  м€ мћ€л‹¤, ліґл©ґ
        <div>{person.address.line1}</div>
        </>
    )
}

