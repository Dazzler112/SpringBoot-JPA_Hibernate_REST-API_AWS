const person = {
    name: 'Ranga Karanam',
    address: {
        line1: 'Baker Street',
        city: 'London',
        country: 'UK',
    },
    profiles: ['twitter', 'linkedin', 'instagram'],
    
    рџџў н™”м‚ґн‘њ н•Ём€
    //9: м—¬кё° к°ќмІґ м•€м—ђлЏ„ л№„мЉ·н•њ к±ё н•  м€ мћ€л‹¤, н™”м‚ґн‘њ мЉ¤нѓЂмќјлЎњ м •мќн•  кІѓмќґл‹¤, мќґк±ґ Javaм—ђм„њ Lambdaн•Ём€лҐј м •мќн•лЉ” кІѓкіј л§¤мљ° л№„мЉ·н•л‹¤, к°ќмІґмќ мќјл¶ЂлЎњ н•Ём€лҐј м •мќн•лЉ” к±°м§Ђ
    //м•„лћмќ н•Ём€мќ мќґл¦„мќґ м—¬кё° л“¤м–ґк°Ђл‹€к№Њ printProfileмќґ м—¬кё° м†Ќм„± мќґл¦„мќґ лђлЉ” к±°кі  м—¬кё° н™”м‚ґн‘њ н•Ём€к°Ђ л“¤м–ґк°ЂлЉ” кІѓмќґл‹¤ м—ґкі ()л‹«лЉ” кґ„нём•€м—ђ нЊЊлќјлЇён„°лҐј л„Јкі  н™”м‚ґн‘њлҐј м“ґл‹¤ Lambda н•Ём€лҐј м •мќн•лЉ” кІѓкіј л№„мЉ·н•л‹¤
    //мќјл°м ЃмњјлЎњ н™”м‚ґн‘њ н•Ём€лҐј м •мќн•лЉ” к°ЂмћҐ мў‹мќЂ л°©лІ•мќЂ л°”лЎњ м¤‘кґ„нёмќґл‹¤, м¤‘кґ„нёлҐј м—ґкі  л‹«мќЂ л‹¤мќЊ мќґ мЅ”л“њлҐј м—¬кё°м—ђ м®кё°кІ л‹¤ (м•„лћмќ н•Ём€ console.logлҐј м—¬кё° м¤‘кґ„нё м•€мњјлЎњ м®кё°кё°)
     printProfile: () => {
        //9-1: console.logлЎњ person.profiles[0]мќ„ ліґкІ л‹¤ м•„лћмќ н•Ём€лЉ” м§Ђмљ°кІ л‹¤, мќјл° н•Ём€м™Ђ н™”м‚ґн‘њ н•Ём€мќ м°ЁмќґлҐј ліґм—¬мЈјл ¤кі  л§Њл“  к±°л‹€к№Њ 
        console.log(person.profiles[0])
    }
    //printProfile н•Ём€ м„¤лЄ…: printProfileмќґлќјлЉ” н™”м‚ґн‘њ н•Ём€к°Ђ м •мќлђм—€кі  лЎњк·ёлЎњ person.profilesлҐј м¶њл Ґн•кі  мћ€л‹¤
    //мќґм њ LearningJavaScriptм»ґнЏ¬л„ЊнЉём—ђ printProfileмќ„ нём¶њн•ґліґмћђ //9-2:LearningJavaScript м»ґнЏ¬л„ЊнЉёлЎњ..
}

    /* <== м—¬кё°м„  мЈјм„ќмІл¦¬ н•м§Ђл§Њ м‹¤м њлЎњлЉ” м‚­м њн•Ё
    function printProfile () {  
    }*/

export default function LearningJavaScript () {
    return(
        <>
        <div>{person.name}</div>
        <div>{person.address.line1}</div>
        <div>{person.profiles[0]}</div>

        //9-2: м–ґл–»кІЊ н• к№Њ? м¤‘кґ„нё м•€м—ђ л„ЈлЉ”л‹¤, person.printProfileм—ђл‹¤к°Ђ кґ„нёлҐј м¶”к°Ђн•њл‹¤, н•Ём€лҐј нём¶њн•кі  м‹¶л‹¤ м¤‘кґ„нё м•€м—ђ person.printProfile()н•кі , м ЂмћҐн•кі  м–ґл–»кІЊ лђлЉ”м§Ђ ліґмћђ
        <div>{person.printProfile()}</div>
        </>
    )
}
