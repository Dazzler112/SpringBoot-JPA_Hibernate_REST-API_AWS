вЏє vsc


вЏє ListTodosComponent.jsx



import { useEffect, useState } from "react"
import { retrieveAllTodosForUsername } from "./api/TodoApiService"

function ListTodosComponent() {

    const today = new Date()

    const targetDate = new Date(today.getFullYear() + 12, today.getMonth(), today.getDay())

    const [todos, setTodos] = useState([])

    useEffect( () => refreshTodos() , [] )

    function refreshTodos() {

    //8: TodoApiService.jsм—ђм„њ мќґл¦„мќґ ліЂкІЅлђђмњјл‹€ м—¬кё°лЏ„ ліЂкІЅ.. retrieveAllTodosForUsername=> retrieveAllTodosForUsernameApiлЎњ.. //8-1: deleteTodo()лЎњ..
    retrieveAllTodosForUsernameApi('in28minutes')
        .then(response => {
            setTodos(response.data)
            }
        )
        .catch(error => console.log(error))
    }    

    //8-1: м—¬кё°м„њлЏ„ л‹¤лҐё к±ё м‚¬мљ©н•  кІѓмќґл‹¤, к·ёлџј мљ°л¦¬лЉ” deleteTodoApi()лЏ„ м‚¬мљ©н•  к±°кі ,
    function deleteTodo(id) {
        console.log('clicked ' + id);
      //8-2: мќґ deleteTodoApi()м—ђ мІ« лІ€м§ё мќём€лЎњм„њ usernameмќ„ л„Јмњјл ¤кі  н•њл‹¤, мќём€лҐј мЎ°м‹¬н•ґм•ј н•њл‹¤ usernameмќЂ in28minutesкі , мћ м‹њ мљ°л¦¬лЉ” in28minutesлҐј н•л“њмЅ”л”©н•кі  мћ€м—€лЉ”лЌ° н•л“њмЅ”л”©мќ„ л‹¤мќЊ л‹Ёкі„м—ђм„њ м њк±°н•ґм•ј н•њл‹¤
      //м§Ђкё€мќЂ мљ°м„  м‚­м њлҐј мћ‘лЏ™м‹њн‚¤лЉ” лЌ° м§‘м¤‘н•ґліґмћђ, к·ёлџј deleteTodoApi('in28minutes', id)лќј н•кі , 
        deleteTodoApi('in28minutes', id)
        //8-3: м„±кіµм Ѓмќё мќ‘л‹µмќґ лђлЏЊм•„м¤л©ґ мљ°л¦° л­ н•л ¤кі  н–€м§Ђ
        .then(
          //8-4: м—¬кё°м„њ к·ёк±ё н• кІѓмќґл‹¤
        )
          //8-5: к·ёл¦¬кі  .catch()лЏ„ к°™мќ„ кІѓмќґл‹¤
        .catch(
          
        )
    }

    return (
        <div className='container'>
            <h1>Things You Want To Do!</h1>
            <div>
                <table className='table'>
                    <thead>
                        <tr>
                            <th>Description</th>
                            <th>is Done?</th>
                            <th>Target Date</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        {
                            todos.map(
                                todo => (
                                    <tr key={todo.id}>
                                        <td>{todo.description}</td>
                                        <td>{todo.done.toString()}</td>
                                        {/* <td>{todo.targetDate.toDateString()}</td> */}
                                        <td>{todo.targetDate.toString()}</td>
                                        <td> <button className="btn btn-warning" 
                                                     onClick={() => deleteTodo(todo.id)}>Delete</button> </td>
                                   </tr>
                                )
                            )
                        }
                    </tbody>
                </table>
            </div>
        </div>
    )
}

export default ListTodosComponent
