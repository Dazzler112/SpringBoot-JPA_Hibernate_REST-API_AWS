вЏє vsc


вЏє ListTodosComponent.jsx




import { useEffect, useState } from "react"
import { deleteTodoApi, retrieveAllTodosForUsername, retrieveAllTodosForUsernameApi } from "./api/TodoApiService"

function ListTodosComponent() {

    const today = new Date()

    const targetDate = new Date(today.getFullYear() + 12, today.getMonth(), today.getDay())

    const [todos, setTodos] = useState([])
   
    //10: к·ёлџј м—¬кё°м„њ Todosм™Ђ л№„мЉ·н•кІЊ мѓЃнѓњлҐј н•л‚ лЌ” л§Њл“¤ кІѓмќґл‹¤
    //к·ёлџј м—¬кё°м—ђ useState(null)мќґлќјкі  н•кі  todosлҐј messageлЎњ ліЂкІЅн•њл‹¤, к·ёл¦¬кі  setTodosлҐј setMessageлЎњ ліЂкІЅн•ґмЈјмћђ, к·ёл¦¬кі  мѓЃл‹Ём—ђм„њ л©”м‹њм§ЂлҐј н‘њм‹њн•  кІѓмќґл‹¤ //10-1: return м•€мњјлЎњ..
    const [message, setMessage] = useState(null)

    useEffect( () => refreshTodos() , [] )

    function refreshTodos() {
    retrieveAllTodosForUsernameApi('in28minutes')
        .then(response => {
            setTodos(response.data)
            }
        )
        .catch(error => console.log(error))
    }    

    function deleteTodo(id) {
        console.log('clicked ' + id);
        deleteTodoApi('in28minutes', id)
        .then(

            () => {
              //10-2: мќґм њ мљ°л¦¬лЉ” мѓЃнѓњм—ђ л©”м‹њм§ЂлҐј м „м†Ўн•ґм•ј н•њл‹¤ к·ёк±ё м–ґл–»кІЊ н• к№Њ? setMessageлќјкі  н•кі  л°±н‹±(`)мќ„ мЌЁм„њ '${id}лЎњ лђњ TodoлҐј м„±кіµм ЃмњјлЎњ м‚­м њн–€л‹¤ (Delete of todo with ${id} successful)' лќјкі  н•  м€ мћ€м§Ђ
              //м—¬кё° idк°Ђ мћ€л‹€? г…‡г…‹ мљ°л¦° idк°Ђ мћ€м§Ђ (function deleteTodo(id) {..), к·ёлџј мќґкІЊ м„±кіµм ЃмњјлЎњ л©”м‹њм§ЂлҐј н‘њм‹њн•  кІѓмќґл‹¤, м–ґл–»кІЊ лђлЉ”м§Ђ ліјк№Њ?
                setMessage(`Delete of todo with ${id} successful`)

                refreshTodos()
            }
            //1: Display message
            //2: Update Todos list
        ) 
        .catch(
            
        )
    }

    return (
        <div className='container'>
            <h1>Things You Want To Do!</h1>

            //10-1: к·ёлџј Things You Want To Do! л°”лЎњ м•„лћм—ђм„њ мљ°л¦° <div>лҐј м¶”к°Ђн•  к±°кі , м—¬кё° м•€м—ђм„њ л©”м‹њм§ЂлҐј н‘њм‹њн•  кІѓмќґл‹¤
            //к·ёлџј мќґк±ґ м—¬кё°м—ђ л„ЈмќЂ мѓЃнѓњм—ђм„њ messageлҐј мћЎмќ„ к±°кі , "alert alert-warning"мќґлќјлЉ” classNameмќ„ л¶Ђм—¬н•  кІѓмќґл‹¤, мќґк±ё м ЂмћҐн•кі  //10-2: л‹¤м‹њ deleteTodo()лЎњ..
            <div className="alert alert-warning">{message}</div>
            <div>
                <table className='table'>
                    <thead>
                        <tr>
                            <th>Description</th>
                            <th>is Done?</th>
                            <th>Target Date</th>
                            <th>Delete</th>
                        </tr>
                    </thead>
                    <tbody>
                        {
                            todos.map(
                                todo => (
                                    <tr key={todo.id}>
                                        <td>{todo.description}</td>
                                        <td>{todo.done.toString()}</td>
                                        {/* <td>{todo.targetDate.toDateString()}</td> */}
                                        <td>{todo.targetDate.toString()}</td>
                                        <td> <button className="btn btn-warning" 
                                                     onClick={() => deleteTodo(todo.id)}>Delete</button> </td>
                                   </tr>
                                )
                            )
                        }
                    </tbody>
                </table>
            </div>
        </div>
    )
}

export default ListTodosComponent
