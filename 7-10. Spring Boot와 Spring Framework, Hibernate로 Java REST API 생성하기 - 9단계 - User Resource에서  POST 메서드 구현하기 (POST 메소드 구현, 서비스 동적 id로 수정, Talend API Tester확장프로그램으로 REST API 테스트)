package com.in28minutes.rest.webservices.restfulwebservices.user;

import java.time.*;
import java.util.*;
import java.util.function.*;

import org.springframework.stereotype.*;

@Component
public class UserDaoService {
    // JPA/Hibernate > Database
    // UserDaoService > Static List
	
	private static List<User> users = new ArrayList<>();

  //2-3: ë¨¼ì € ë³€ìˆ˜ë¥¼ í•œë‚˜ ìƒì„±í•˜ê² ë‹¤ private static int ê·¸ë¦¬ê³  ì´ë¦„ì€ usersCountë¡œ ê°„ë‹¤ ì‹œì‘ ê°’ì€ 3ìœ¼ë¡œ ê°€ê² ë‹¤, ì´ í”„ë¡œê·¸ë¨ì´ ì‹œì‘ì´ ë˜ë©´ userCountì˜ ê°’ì€ 3ì´ ëœë‹¤ 
//	private static int usersCount = 3;
  //2-3 ë²ˆì™¸: ë˜ë‹¤ë¥¸ ë°©ë²•ìœ¼ë¡œëŠ” 0ì—ì„œë¶€í„° ì‹œì‘í•˜ê³  ì•„ë˜ static{..}ì˜ ì•„ì´ë”” ê°’ì„ ++usersCountë¡œ í• ë‹¹í•œë‹¤
  private static int usersCount = 0;

/*	static {
		users.add(new User(1, "Adam", LocalDate.now().minusYears(30)));
		users.add(new User(2, "Eve", LocalDate.now().minusYears(25)));
		users.add(new User(1, "Jim", LocalDate.now().minusYears(20)));
	} */

  //2-3 ë²ˆì™¸ ê³„ì†: ì²«ë²ˆì§¸ ì‚¬ìš©ì ì•„ì´ë””ëŠ” 1ì´ ë˜ê³ , ë‹¤ìŒì€ 2, ë‹¤ìŒì€ 3ì´ëœë‹¤//2-4: save()ë©”ì†Œë“œë¡œ.. 
	static {
		users.add(new User(++usersCount, "Adam", LocalDate.now().minusYears(30)));
		users.add(new User(++usersCount, "Eve", LocalDate.now().minusYears(25)));
		users.add(new User(++usersCount, "Jim", LocalDate.now().minusYears(20)));
	}
	
  public List<User> findAll() {
		return users;
	}

  //2-1: public User save()ë¡œ, ì¸ìëŠ” Userë¡œë§ì´ë‹¤, ì‚¬ìš©ì ì •ë³´ë¥¼ ì·¨ë“í•œ ë‹¤ìŒì—” ë­˜ í•´ì•¼ í•˜ì§€?
	public User save(User user) {
    //2-4: ì´ê±°ë‘ ë¹„ìŠ·í•˜ê²Œ ì‚¬ìš©ìë¥¼ ì¶”ê°€í•  ë•Œë„ ë§ì´ë‹¤, user.setId()ê·¸ë¦¬ê³  ì¸ìë¡œ ++userCountë¥¼ ë„£ì–´ì¤€ë‹¤, ë‹¤ìŒìœ¼ë¡œ ìƒì„±ë˜ëŠ” ì‚¬ìš©ì ì•„ì´ë””ëŠ” 4ê°€ ë˜ê³  ê·¸ë ‡ê²Œ í•˜ë‚˜ì”© ì¦ê°€ëœë‹¤, ì§€ê¸ˆ ì´ ì‘ì—…ì€ ì„ì‹œ ì‘ì—…ì¼ ë¿ì´ë‹¤ ë‚˜ì¤‘ì—ëŠ” DBì— ìƒì„±ë˜ëŠ” ì‹œí€€ìŠ¤ë¥¼ ì´ìš©í•  ê²ƒì´ë‹¤ JPA/Hibernateë¥¼ ì´ìš©í•´ì„œì´ë‹¤ ì¼ë‹¨ save()ë©”ì†Œë“œë¥¼ êµ¬í˜„í–ˆë‹¤ ë‹¤ì‹œ ëŒì•„ê°€ì„œ//3ìœ¼ë¡œ...
		user.setId(++usersCount);
		//2-2: users.add()ë¥¼ í˜¸ì¶œí•´ì„œ íŠ¹ì • ì‚¬ìš©ìë¥¼ ìƒì„±í•œë‹¤ (user) , ì‚¬ìš©ìë¥¼ ìƒˆì„±í•  ë•Œ ë™ì ìœ¼ë¡œ ì•„ì´ë””ë¥¼ í• ë‹¹í•´ì¤˜ì•¼ í•˜ëŠ”ë° ì—¬ê¸°ì„  ì¹´ìš´í„°ë¥¼ ì“¸ ê²ƒì´ë‹¤, ë¨¼ì € ë³€ìˆ˜ë¥¼ í•˜ë‚˜ ìƒì„±í•˜ê² ë‹¤//2-3: List<User> users ì•„ë˜ì— ì‘ì„±...
	  users.add(user);
		return user;
  }
	
	public User findOne(int id) {
		Predicate<? super User> predicate = user -> user.getId().equals(id);
		return users.stream().filter(predicate).findFirst().get();
	}
	
}

