import { useState } from 'react'
import './Counter.css'

//3-2: 'import {PropTypes}' к°Ђм ём¬ кіімќЂ 'prop-types'мќґл‹¤, мќґл ‡кІЊ н•кі  нѓЂмћ…мќ„ м„ нѓќн•л©ґ лђњл‹¤ //3-3: л‹¤м‹њ Counter.propTypes м•€мњјлЎњ..
import {PropTypes} from 'prop-types'

export default function Counter({by}) {

    // [0, Ж’] 
    // const [firstElt, secondElt] = array
    const [count, setCount] = useState(0);

    function incrementCounterFunction() {

        setCount(count + by)

    }

    function decrementCounterFunction() {
        
        setCount(count - by)
    }

    return(
    <div className='Counter'>
        <span className='count'>{count}</span>
        <div>
         <button className="counterButton" 
                 onClick={incrementCounterFunction}
         >+{by}</button>
        <button className="counterButton" 
                 onClick={decrementCounterFunction}
         >-{by}</button>
        </div>
    </div>
    )
}

//3: л§Ё л°‘м—ђ 'Counter.propTypes'лҐј м¶”к°Ђн•кі  к°ќмІґ н•нѓњлЎњ к°’мќ„ л„Јм–ґм•ј н•њл‹¤, 
Counter.propTypes = {
  //3-1: к°ќмІґ н‘њм‹њлҐј м¶”к°Ђн•кі  нѓЂмћ…мќ„ м§Ђм •н•  н”„лЎњнЌјн‹° мќґл¦„мќ„ л„ЈлЉ”л‹¤, 'by'лҐј л„Јмњјл©ґ лђкІ м§Ђ
  //м§Ђм •н•  нѓЂмћ…мќЂ PropTypesм—ђ м ђ(.)мќ„ м°Ќмњјл©ґ л‚м¤лЉ”кІЊ м—†л„¤.., лЁјм Ђ PropTypesлҐј мћ„нЏ¬нЉён•ґм•ј н•  кІѓ к°™л‹¤//3-2: мњ„мќ мћ„нЏ¬нЉёлЎњ..
  //3-3: нѓЂмћ…мќ„ м€«мћђ(number)лЎњ м§Ђм •н•кі  м ЂмћҐн•кІ л‹¤, л‹¤м‹њ лёЊлќјмљ°м ЂлЎњ лЏЊм•„к°Ђліґл©ґ..
    by: PropTypes.number
}
