import { useState } from 'react'
import './Counter.css'

//3-2: 'import {PropTypes}' ê°€ì ¸ì˜¬ ê³³ì€ 'prop-types'ì´ë‹¤, ì´ë ‡ê²Œ í•˜ê³  íƒ€ì…ì„ ì„ íƒí•˜ë©´ ëœë‹¤ //3-3: ë‹¤ì‹œ Counter.propTypes ì•ˆìœ¼ë¡œ..
import {PropTypes} from 'prop-types'

export default function Counter({by}) {

    // [0, Æ’] 
    // const [firstElt, secondElt] = array
    const [count, setCount] = useState(0);

    function incrementCounterFunction() {

        setCount(count + by)

    }

    function decrementCounterFunction() {
        
        setCount(count - by)
    }

    return(
    <div className='Counter'>
        <span className='count'>{count}</span>
        <div>
         <button className="counterButton" 
                 onClick={incrementCounterFunction}
         >+{by}</button>
        <button className="counterButton" 
                 onClick={decrementCounterFunction}
         >-{by}</button>
        </div>
    </div>
    )
}

//3: ë§¨ ë°‘ì— 'Counter.propTypes'ë¥¼ ì¶”ê°€í•˜ê³  ê°ì²´ í˜•íƒœë¡œ ê°’ì„ ë„£ì–´ì•¼ í•œë‹¤, 
Counter.propTypes = {
  //3-1: ê°ì²´ í‘œì‹œë¥¼ ì¶”ê°€í•˜ê³  íƒ€ì…ì„ ì§€ì •í•  í”„ë¡œí¼í‹° ì´ë¦„ì„ ë„£ëŠ”ë‹¤, 'by'ë¥¼ ë„£ìœ¼ë©´ ë˜ê² ì§€
  //ì§€ì •í•  íƒ€ì…ì€ PropTypesì— ì (.)ì„ ì°ìœ¼ë©´ ë‚˜ì˜¤ëŠ”ê²Œ ì—†ë„¤.., ë¨¼ì € PropTypesë¥¼ ì„í¬íŠ¸í•´ì•¼ í•  ê²ƒ ê°™ë‹¤//3-2: ìœ„ì˜ ì„í¬íŠ¸ë¡œ..
  //3-3: íƒ€ì…ì„ ìˆ«ì(number)ë¡œ ì§€ì •í•˜ê³  ì €ì¥í•˜ê² ë‹¤, ë‹¤ì‹œ ë¸Œë¼ìš°ì €ë¡œ ëŒì•„ê°€ë³´ë©´..
    by: PropTypes.number
}
