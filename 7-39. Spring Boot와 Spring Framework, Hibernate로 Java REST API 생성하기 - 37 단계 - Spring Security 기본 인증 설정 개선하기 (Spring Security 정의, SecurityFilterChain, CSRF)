вЏє SpringSecurityConfiguration нЃґлћмЉ¤нЊЊмќј



package com.in28minutes.rest.webservices.restfulwebservices.security;

import org.springframework.context.annotation.*;
import org.springframework.security.config.annotation.web.builders.*;
import org.springframework.security.web.*;

@Configuration
                                        //2-1:мќґ кµ¬л¬ёмњјлЎњ м€м™ёлҐј лЌм§ЂкІЊ лђњл‹¤, мќґм њ м ЂмћҐн•кі 
public class SpringSecurityConfiguration throws Exception {

	@Bean
	public SecurityFilterChain filterChain(HttpSecurity http) {

    //2: мІл¦¬лђм§Ђ м•ЉмќЂ м€м™ёлќјкі  лђм–ґмћ€л‹¤ ( Unhandled exception type Exception ) Ctrl+1мќ„ л€Њлџ¬ throws м„ м–ё м¶”к°ЂлҐј м„ нѓќн•њл‹¤ 
		return http.build();
	}
}

