вЏє SpringSecurityConfiguration нЃґлћмЉ¤нЊЊмќј



package com.in28minutes.rest.webservices.restfulwebservices.security;

import org.springframework.context.annotation.*;
import org.springframework.security.config.annotation.web.builders.*;
import org.springframework.security.web.*;

@Configuration
public class SpringSecurityConfiguration {

	@Bean
	public SecurityFilterChain filterChain(HttpSecurity http) throws Exception {

  //3: мЈјм„ќмІл¦¬н•кі  м—¬кё° мѓЃл‹Ём—ђ мњ„ л‹Ёкі„ м„¤лЄ…мќ„ л„ЈкІ л‹¤ (н•л©ґм„њ м°ёмЎ°н•  м€ мћ€кІЊ), н•л‚м”© кµ¬н„н•ґліґмћђ, м њмќј лЁјм Ђ н•  мќјмќЂ лЄЁл“  мљ”мІ­мќґ мќём¦ќлђлЏ„лЎќ н•лЉ” кІѓмќґл‹¤,
//		1) All requests should be authenticated

      //3-1: http.authorizeHttpRequests()лҐј мћ…л Ґн•њл‹¤, м—¬кё°м—ђ лЎњм§Ѓмќ„ м •мќн•ґм•јкІ м§Ђ
      http.authorizeHttpRequests(
                //3-2: ліЂм€лЄ…мќ„ authлЎњ н•кі  лћЊл‹¤ н‘њн„м‹ќмќ„ м •мќн•њл‹¤ auth.anyRequestлќјкі  мћ…л Ґн•л©ґ л“¤м–ґм¤лЉ” мљ”мІ­мќЂ м „л¶Ђ мќём¦ќмќ„ л°›кІЊ лђкІ м§Ђ мќґкІЊ 1л‹Ёкі„кі 
                //мќґл ‡кІЊ лЄЁл“  мљ”мІ­мќ„ мќём¦ќн•кІЊ лђњл‹¤
                auth -> auth.anyRequest().authenticated()
                );
                //3-2 кі„м†Ќ: к·ёлџ¬л‹€к№Њ 1л‹Ёкі„лЎњ лЄЁл“  мљ”мІ­мќґ мќём¦ќмќ„ л°›м•„м•ј н•њл‹¤ м ЂмћҐн•кі  мѓ€лЎњкі м№Ё н•ґліґмћђ
//	  2) If a request is not authenticated, a web page is shown
//	  3) CSRF -> POST , PUT

		return http.build();
	}
}


