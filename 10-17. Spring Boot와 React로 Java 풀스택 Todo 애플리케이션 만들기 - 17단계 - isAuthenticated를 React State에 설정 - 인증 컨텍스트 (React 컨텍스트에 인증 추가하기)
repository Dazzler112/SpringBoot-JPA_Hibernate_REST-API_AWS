вЏє AuthContext.js




import { createContext, useContext, useState } from "react";

// Create a Context
export const AuthContext = createContext()

export const useAuth = () => useContext(AuthContext)


// Share the created context with other components
export default function AuthProvider({ children }) {

    //Put some state in the context
    const [number, setNumber] = useState(10)

    //1: const isAuthenticated, setAuthenticatedлҐј м„ м–ён•њл‹¤, мў‹л‹¤ мќём¦ќ м—¬л¶Ђмќ кё°ліёк°’мќЂ falseмќґл‹¤
    //м»Ён…ЌмЉ¤нЉём—ђ мѓЃнѓњ к°’мќ„ м ЂмћҐн•лЉ” isAuthenticated ліЂм€лҐј л§Њл“¤кі  ліЂм€лҐј м—…лЌ°мќґнЉён•лЉ” л©”м„њл“њлЏ„ л§Њл“¤м—€м§Ђ isAuthenticated, setAuthenticated к·ёл¦¬кі  useState(false) мў‹л‹¤
    //к·ёлџ¬л©ґ мќґ к°’мќ„ л‹¤лҐё м»ґнЏ¬л„ЊнЉём—ђм„њлЏ„ м‚¬мљ©н•  м€ мћ€кІЊ н•кі  м‹¶л‹¤, м–ґл–»кІЊ н•м§Ђ? valueм—ђ м¶”к°Ђн•л©ґ лђњл‹¤ //1-1лЎњ..
    const [isAuthenticated, setAuthenticated] = useState(false)

    setInterval(() => setNumber(number + 1), 10000)

    return (

        //1-1: м—¬кё° number м™ём—ђлЏ„ мљ°л¦¬лЉ” isAuthenticatedлҐј м¶”к°Ђн•њл‹¤, мќґм њ л‹¤лҐё м»ґнЏ¬л„ЊнЉёк°Ђ isAuthenticatedм—ђ м ‘к·јн•  м€ мћ€л‹¤
        //к·ёл¦¬кі  setAuthenticatedм—ђлЏ„ м ‘к·јн•кІЊ н•кІ л‹¤, лЎњк·ёмќё м»ґнЏ¬л„ЊнЉём—ђм„  setAuthenticatedм—ђ м ‘к·јн•ґм•јкІ м§Ђ, м‚¬мљ©мћђк°Ђ мќём¦ќлђм—€лЉ”м§Ђ м—¬л¶ЂлҐј м„¤м •н•кё° мњ„н•ґм„њл‹¤ 
        <AuthContext.Provider value={ {number, isAuthenticated, setAuthenticated} }>
            {children}
        </AuthContext.Provider>
    )
}
