import { useState } from 'react'
import './Counter.css'

export default function Counter() {

    // [0, Ж’] 
    //4: л°°м—ґмќ мІ« лІ€м§ё к°’м—ђлЉ” л°н™ к°’мќ„ м ЂмћҐн•  ліЂм€ мќґл¦„мќ„ л„ЈлЉ”л‹¤ 'count'лЎњ н•кІ л‹¤, countлЉ” ліЂм€ мќґл¦„мќґл‹¤, к·ёл¦¬кі  н•Ём€ мќґл¦„мњјлЎњ 'setCount'лҐј л„ЈлЉ”л‹¤
    // мќґл ‡кІЊ мћ‘м„±н•л©ґ н›…м—ђм„њ л°°м—ґмќґ л°н™лђм—€мќ„ л•Њ, н•ґл‹№ л°°м—ґмќ к°Ѓ мљ”м†Њк°Ђ лђ л‹¤лҐё л°°м—ґмќ к°Ѓ мљ”м†Њм—ђ л§¤н•‘лђњл‹¤ 
    //countм—ђлЉ” useState()м—ђм„њ л°н™лђњ мІ« лІ€м§ё мљ”м†Њ, м¦‰ н„мћ¬ мѓЃнѓњ к°’мќґ м ЂмћҐлђкі , setCountм—ђлЉ” мѓЃнѓњ к°’мќ„ м—…лЌ°мќґнЉён•лЉ” н•Ём€к°Ђ м ЂмћҐлђњл‹¤,
    //countм™Ђ setCount к°Ѓк°Ѓм—ђ м ЂмћҐлђњл‹¤, мќґл ‡кІЊ н•л©ґ мЅ”л“њк°Ђ н›Ём”¬ к°„кІ°н•ґм§„л‹¤ //4-1: incrementCounterFunction()н•Ём€лЎњ..
    const [count, setCount] = useState(0);

    function incrementCounterFunction() {

        //4-1: м „м—ђ мЌјлЌ к±ґ м§Ђмљ°кі  setCount()м—ђ count+1мќ„ л„Јкі  м ЂмћҐн•кІ л‹¤
        setCount(count + 1)
    
        /* <==м—¬кё°м„њл§Њ мЈјм„ќмІл¦¬ м‹¤м њмЅ”л“њлЉ” м‚­м њн•Ё
        state[1](state[0] + 1 )
        console.log(state[0])
        console.log(state[1])
        */
        console.log('increment clicked')
    }
    

    return(
    <div className='Counter'>
        <span className='count'>0</span>
        <div>
         <button className="counterButton" 
                 onClick={incrementCounterFunction}
         >+1</button>
        </div>
    </div>
    )
}
