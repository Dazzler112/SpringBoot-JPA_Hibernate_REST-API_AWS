вЏє VSC


вЏє Counter.jsx



import './Counter.css'

export default function Counter() {

  //1: мѓЃнѓњлЉ” useStateлќјлЉ” н›…мќ„ м‚¬мљ©н•ґ м¶”к°Ђн•  м€ мћ€л‹¤, useState() н›…м—ђлЉ” мќёмћђлЎњ кё°ліёк°’мќ„ л„кёё м€ мћ€л‹¤
  //мљ°л¦¬к°Ђ м№ґмљґн„°мќ кё°ліёк°’мњјлЎњ м‚¬мљ©н•  к°’мќЂ '0'мќґм§Ђ, к·ёл¦¬кі  useState()лЉ” л‘ђ к°Ђм§Ђ к°’мќ„ л°н™н•њл‹¤, 
  //к·ёлћм„њ 'state'лќјлЉ” ліЂм€лҐј м„ м–ён•ґ useState()мќ л°н™ к°’мќ„ м ЂмћҐн•  кІѓмќґл‹¤, 
  //к·ёлџ° л‹¤мќЊ л°‘м—ђ мћ€лЉ” incrementCounterFunction()мњјлЎњ к°Ђм„њ console.log()лЎњ stateлҐј м¶њл Ґн•кІ л‹¤ //1-1: incrementCounterFunction н•Ём€лЎњ..
  const state = useState(0);

    function incrementCounterFunction() {
        //1-1: console.log(state)лҐј м‚¬мљ©, к·ёл¦¬кі  лёЊлќјмљ°м ЂлЎњ к°Ђліґмћђ
        console.log('increment clicked')
    }
    

    return(
    <div className='Counter'>
        <span className='count'>0</span>
        <div>
         <button className="counterButton" 
                 onClick={incrementCounterFunction}
         >+1</button>
        </div>
    </div>
    )
}
