вЏє VSC


вЏє AuthContext.js



import { createContext, useState } from "react";

// Create a Context
export const AuthContext = createContext()

//2-1: л€„кµ°к°Ђ м»ґнЏ¬л„ЊнЉёлҐј м‚¬мљ©н•кі мћђ н•л©ґ м‚¬мљ©н•  м€ мћ€лЉ” к°„л‹Ён•њ hookмќ„ мѓќм„±н•лЉ” кІѓмќґл‹¤
//м—¬кё°м—ђм„њ мќґл ‡кІЊ н•мћђ, const useAuth м–ґл””м—ђм„њл“  useAuthлќјкі  н•л©ґ useContextм—ђ м ‘к·јн•  м€ мћ€лЏ„лЎќ л§ђмќґм§Ђ
//м—¬кё° н™”м‚ґн‘њ н•Ём€лҐј л§Њл“¤м–ґ useContextлҐј м“°кІ л‹¤, к·ёл¦¬кі  мќґ constлҐј export н•њл‹¤ 
export const useAuth = () => useContext(AuthContext)


// Share the created context with other components
export default function AuthProvider({ children }) {

    //Put some state in the context
    const [number, setNumber] = useState(10)

    setInterval(() => setNumber(number + 1), 10000)

    return (
        <AuthContext.Provider value={ {number} }>
            {children}
        </AuthContext.Provider>
    )
}

