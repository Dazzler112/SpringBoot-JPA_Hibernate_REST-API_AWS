import { createContext, useState } from "react";

// Create a Context
export const AuthContext = createContext()


// Share the created context with other components
export default function AuthProvider({ children }) {

    //Put some state in the context
    const [number, setNumber] = useState(10)


    //1: setIntervalмќґлќјлЉ” н•Ём€лҐј м‚¬мљ©н•  кІѓмќґл‹¤, setIntervalмќ„ м‚¬мљ©н•л©ґ мќґ н•Ём€лҐј лЄ‡ мґ€л§€л‹¤ м‹¤н–‰н•лќјкі  м§Ђм‹њн•  м€ мћ€л‹¤
    setInterval(
        //1-1: 10мґ€л§€л‹¤ мќґ н•Ём€лҐј нём¶њн•лќјкі  н•  м€ мћ€лЉ” к±°м§Ђ, мќґ нЉ№м • н•Ём€лЉ” numberмќ к°’мќ„ м—…лЌ°мќґнЉён•ґм•ј н•њл‹¤
        //н™”м‚ґн‘њ н•Ём€лҐј м‚¬мљ©н•ґ м—¬кё° н•Ём€лҐј м •мќн•кІ л‹¤ setNumberм—ђ number + 1мќ„ м¤Ђл‹¤
        () => setNumber(number + 1),10000
        
        //м •л¦¬ => мљ°л¦¬к°Ђ н•  мќјмќЂ м•„мЈј к°„л‹Ён•л‹¤, 10мґ€л§€л‹¤ numberмќ к°’мќ„ 1м”© м¦ќк°Ђм‹њн‚¤лЉ” к±°м§Ђ, JavaScriptмќ setInterval н•Ём€лЎњ н•њл‹¤
        //м‚¬м‹¤ м—¬кё° 10мќЂ л°Ђл¦¬м„ём»Ёл“њм—¬м„њ 10мґ€л§€л‹¤ н•кі  м‹¶мњјл©ґ 10,000мњјлЎњ л°”кї”м¤м•ј н•њл‹¤, м ЂмћҐ
    )

    return (
        <AuthContext.Provider value={ {number} }>
            {children}
        </AuthContext.Provider>
    )
}

