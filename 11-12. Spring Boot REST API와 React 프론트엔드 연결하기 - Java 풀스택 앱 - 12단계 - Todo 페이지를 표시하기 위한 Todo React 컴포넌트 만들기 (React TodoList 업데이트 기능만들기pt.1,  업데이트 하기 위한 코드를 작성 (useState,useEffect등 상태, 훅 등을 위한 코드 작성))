import {useParams} from 'react-router-dom'
import { retrieveTodoApi } from './api/TodoApiService'
import { useAuth } from './security/AuthContext'


export default function TodoComponent() {

    const {id} = useParams()

    const authContext = useAuth()
    
    const username = authContext.username

    //9: ìš°ë¦¬ëŠ” Todo í˜ì´ì§€ë¥¼ ë¡œë”©í•  ë•Œ retrieveTodos()ë¥¼ í˜¸ì¶œí•˜ë ¤ê³  í•œë‹¤, ê·¸ëŸ¼ ì•ì—ì„œ í•´ì¤€ ëŒ€ë¡œ
    //useEffectë¥¼ ì“¸ ìˆ˜ ìˆë‹¤, ê·¸ëŸ¼ useEffect()í›…ì—ì„œ ìš°ë¦¬ëŠ” retrieveTodosApië¥¼ í˜¸ì¶œí•  ìˆ˜ ìˆë‹¤
    useEffect(
        //9-1: ê·¸ëŸ¼ ì—¬ê¸°ì„œ í™”ì‚´í‘œ í•¨ìˆ˜ë¥¼ ë§Œë“¤ê³  retrieveTodos()ë¥¼ í˜¸ì¶œí•˜ê² ë‹¤
        () => retrieveTodos(),
        //9-2: ê·¸ë¦¬ê³  ë„ˆëŠ” id ê°’ì´ ë³€ê²½ë  ë•Œì—ë§Œ ì´ê²Œ ìƒˆë¡œê³ ì¹¨ ë˜ë„ë¡ í•˜ë ¤ê³  í•œë‹¤ ê·¸ëŸ¼ ë„ˆëŠ” ë‘ ë²ˆì§¸ ê°’ì„ id ë°°ì—´ë¡œì„œ ë„£ì–´ì¤„ ê±°ê³ , ì´ì œ ë¬¸ì œ ì—†ëŠ” ê²ƒ ê°™ë‹¤, ì´ê±¸ ì €ì¥í•´ë³´ì
        [id]
    )    

    function retrieveTodos() {
        retrieveTodoApi(username, id)
        .then(response => (
            console.log(response)
            )
