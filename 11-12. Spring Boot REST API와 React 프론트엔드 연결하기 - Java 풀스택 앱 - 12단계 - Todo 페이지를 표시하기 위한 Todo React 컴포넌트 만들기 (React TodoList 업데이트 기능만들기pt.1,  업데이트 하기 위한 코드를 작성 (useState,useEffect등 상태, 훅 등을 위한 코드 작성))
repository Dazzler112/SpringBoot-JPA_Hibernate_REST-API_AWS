import {BrowserRouter, Routes, Route, Navigate} from 'react-router-dom'
import './TodoApp.css'
import LogoutComponent from './LogoutComponent'
import HeaderComponent from './HeaderComponent'
import ListTodosComponent from './ListTodosComponent'
import ErrorComponent from './ErrorComponent'
import WelcomeComponent from './WelcomeComponent'
import LoginComponent from './LoginComponent'

//3-2: ê·¸ëŸ¬ë©´ TodoComponentê°€ ì„í¬íŠ¸ë  ê²ƒì´ë‹¤, ê·¸ëŸ°ë° TodoComponentê°€ ì„í¬íŠ¸ë˜ì§€ ì•Šì•˜ë‹¤ ë¬´ì—‡ì´ ì˜ëª»ë˜ì—ˆëŠ”ì§€ ë³´ì, todocomponentë¥¼ ê²€ìƒ‰í•´ì„œ ë³´ë©´..

import AuthProvider, { useAuth } from './security/AuthContext'


function AuthencatedRoute({children}) {
    const authContext = useAuth()
    if(authContext.isAuthenticated)
        return children

    return <Navigate to="/" />
}

export default function TodoApp() {
    return(
        <div className='TodoApp'>
            <AuthProvider>
            <BrowserRouter>
                <HeaderComponent />
                <Routes>
                    <Route path='/' element={<LoginComponent />} />
                    <Route path='/login' element={<LoginComponent />} />

                    <Route path='/welcome/:username' element={
                        <AuthencatedRoute>
                            <WelcomeComponent />
                        </AuthencatedRoute>
                        } />

                    <Route path='/todos' element={
                        <AuthencatedRoute>
                            <ListTodosComponent />
                        </AuthencatedRoute>
                        } />

                    //3: TodoComponentë¥¼ ì¶”ê°€í•  ê²ƒì´ë‹¤, ê·¸ëŸ¼ ì•ì—ì„œ í•œ ê²ƒê³¼ ê°™ì€ ê±¸ í•  ê±°ê³  ìš°ë¦° idë¥¼ ë„£ì–´ì„œ /todo/ë¼ëŠ” ê²½ë¡œë¥¼ ì„¤ì •í•˜ë ¤ê³  í•œë‹¤
                    //ê·¸ëŸ¼ ë„ˆëŠ” Todo idë¥¼ íŒŒë¼ë¯¸í„°ë¡œì„œ ë„£ì–´ì£¼ë ¤ê³  í•˜ì§€, ê·¸ëŸ¼ '/todo/:id'ë¼ê³  í•˜ê³  
                    <Route path='/todo/:id' element={
                        <AuthencatedRoute>
                          //3-1: elementëŠ” TodoComponentê°€ ë  ê²ƒì´ë‹¤ ctrl+spaceë¥¼ ëˆ„ë¥´ê³  ì—”í„° í‚¤ë¥¼ ëˆŒëŸ¬ë³´ì //3-2: ì„í¬íŠ¸ë¡œ..
                            <TodoComponentt />
                        </AuthencatedRoute>
                        } />

                    <Route path='/logout' element={
                        <AuthencatedRoute>
                            <LogoutComponent />
                        </AuthencatedRoute>
                        } />

                    <Route path='*' element={<ErrorComponent />} />
                </Routes>
            </BrowserRouter>
            </AuthProvider>
        </div>
    )
}
