вЏє VSC


вЏє TodoApp.jsx


//4-1: м—¬кё° importк°Ђ мѓќкё°лЉ” к±ё ліј м€ мћ€м§Ђ, л¦¬м•ЎнЉём—ђм„њ useStateлҐј мћ„нЏ¬нЉён•кі  мћ€л‹¤ //4-2: л‹¤м‹њ LoginComponent() н•Ём€лЎњ..
import { useState } from 'react'
import './TodoApp.css'

export default function TodoApp() {
    return(
        <div className='TodoApp'>
            <LoginComponent />
            {/* <WelcomeComponent /> */}
        </div>
    )
}

function LoginComponent() {

    //4: useState н•л©ґ лђњл‹¤, useStateлҐј м¶”к°Ђн•њл‹¤ //4-1: importлЎњ ..
    //4-2: кё°ліё к°’мќ„ in28minutesлЎњ н•кі  м‹¶л‹¤, useStateмќ к°’мњјлЎњ in28minutesлҐј мЈјкІ л‹¤, мќґк±ґ л‘ђ к°њ мљ”м†Њк°Ђ мћ€лЉ” л°°м—ґмќ„ л°н™н•  кІѓмќґл‹¤, constлЎњ м •мќн•кІ л‹¤
    // мІ« лІ€м§ёлЉ” мќґ State к°’м—ђ м ‘к·јн•  м€ мћ€лЉ” ліЂм€мќґл‹¤ м—¬кё°м—ђлЉ” usernameмќґ л“¤м–ґк°Ђм•јкІ л‹¤
    //к·ёл¦¬кі  usernameмќ„ м—…лЌ°мќґнЉён•  л©”м„њл“њк°Ђ н•„мљ”н•л‹¤, Stateмќ usernameмќ„ ліЂкІЅн•ґм¤„ кІѓмќґл‹¤, мќґ л©”м„њл“њлҐј нём¶њн•л©ґ Stateм—ђ м ЂмћҐлђњ к°’мќ„ м—…лЌ°мќґнЉён•  кІѓмќґл‹¤
    //мќґм њ usernameкіј setUsername л©”м„њл“њлҐј useStateлЎњ к°Ђм ём™”л‹¤
    const [username, setUsername] = useState('in28minutes')

    return (
        <div className='Login'>
            <div className='LoginForm'>
                <div>
                    <label>User Name:</label>

                    //4-3: к·ёлџј мќґм њ м—¬кё°м—ђм„њ username к°’мќ„ м‚¬мљ©н•ґліґмћђ, м—¬кё° in28minutesлҐј м§Ѓм ‘ мћ…л Ґн•лЉ” лЊЂм‹  Stateм—ђ м ‘к·јн•л©ґ лђкІ м§Ђ
                    //м—¬кё° {username}мќ„ л„Јм–ґм¤Ќл‹€л‹¤ ,м–‘м‹ќ мљ”м†ЊлҐј мќґ Stateм™Ђ л¬¶м–ґмЈјлЉ” кІѓмќґл‹¤, м ЂмћҐн•кі  м–‘м‹ќмњјлЎњ лЏЊм•„к°Ђм„њ мѓ€лЎњ кі м№Ё н•ґліґкІ л‹¤
                    <input type="text" name="username" value="{username}" />
                </div>
                <div>
                    <label>Password:</label>
                    <input type="password" name="password"/>
                </div>
                <div>
                    <button type="button" name="login">login</button>
                </div>
            </div>
        </div>
    )
}

function WelcomeComponent() {
    return (
        <div className='Welcome'>
            Welcome Component
        </div>
    )
}

