import { useState } from 'react'
import './TodoApp.css'

export default function TodoApp() {
    return(
        <div className='TodoApp'>
            <LoginComponent />
            {/* <WelcomeComponent /> */}
        </div>
    )
}

function LoginComponent() {

    const [username, setUsername] = useState('in28minutes')

    //8-2: Stateм—ђ passwordлҐј м ЂмћҐн•ґм•ј н•њл‹¤, н•ґліґмћђ, м—¬кё°м—ђм„њpassword, м—¬кёґ setPassword кІ м§Ђ л№„л°ЂлІ€нё мґ€кё° к°’мќЂ л№„м›Њл‘ђкІ л‹¤//8-3: л‹¤м‹њ handlePasswordChange() н•Ём€лЎњ..
    const [password, setPassword] = useState('')

    function handleUsernameChange(event) {
 //       console.log(event.target.value); (м—¬кё°м„њл§Њ мЈјм„ќмІл¦¬ м‹¤м њ мЅ”л“њлЉ” м‚­м њ)
        setUsername(event.target.value);
    }

    //8: handlePasswordChange, handleUsernameChangeм™Ђ handlePasswordChange м—ђ мћ€лЌ лЎњк·ё м¶њл ҐмќЂ м‚­м њн•кі //8-1: returnм•€мњјлЎњ..
    function handlePasswordChange(event) {
        //8-3: мќґ л©”м„њл“њ мќґл¦„мќЂ setPasswordлЎњ н•кІ л‹¤, лђђл‹¤
        setPassword(event.target.value);
    }

    return (
        <div className='Login'>
            <div className='LoginForm'>
                <div>
                    <label>User Name:</label>
                    <input type="text" name="username" value={username} onChange={handleUsernameChange} />
                </div>
                <div>
                    <label>Password:</label>

                    //8-1: м—¬кё°м—ђлЏ„ л№„мЉ·н•њ к±ё м¶”к°Ђн•ґм¤м•ј н•њл‹¤ valueлЉ” passwordлќјкі  н•ґм•јм§Ђ л©”м†Њл“њ мќґл¦„мќЂ handlePasswordChangeлќј н•кІ л‹¤
                    //н•л‚ лЌ” м¶”к°Ђн•  кІЊ мћ€м§Ђ, Stateмќґл‹¤//8-2: const [username, setUsername]м•„лћм—ђм„њ мћ‘м„±..
                    <input type="password" name="password" value={password} onChange={handlePasswordChange}/>
                </div>
                <div>
                    <button type="button" name="login">login</button>
                </div>
            </div>
        </div>
    )
}

function WelcomeComponent() {
    return (
        <div className='Welcome'>
            Welcome Component
        </div>
    )
}
