import { useState } from 'react'
import './TodoApp.css'

export default function TodoApp() {
    return(
        <div className='TodoApp'>
            <LoginComponent />
            {/* <WelcomeComponent /> */}
        </div>
    )
}

function LoginComponent() {

    const [username, setUsername] = useState('in28minutes')

    //5-1: лЎњк·ёмќё м»ґнЏ¬л„ЊнЉё м•€м—ђ handleUsernameChangeлҐј л§Њл“¤кі  нЊЊлќјлЇён„°лЎњ eventлҐј л°›кІ л‹¤
    function handleUsernameChange(event) {
        //5-2: eventмќ лЎњк·ёлҐј м¶њл Ґн•ґліґкІ л‹¤ (console.log(event)), м—¬кё°м—ђ ліЂн™”к°Ђ мѓќкё°л©ґ мќґлІ¤нЉёк°Ђ л°њмѓќн•кі  мќґлІ¤нЉё лЎњк·ёлҐј м—¬кё°м—ђ м¶њл Ґн•лЉ” кІѓмќґл‹¤, м–ґл–»кІЊ лђлЉ”м§Ђ ліґкІ л‹¤
        console.log(event);
    }

    return (
        <div className='Login'>
            <div className='LoginForm'>
                <div>
                    <label>User Name:</label>

                    //5: onChange л©”м„њл“њлҐј м¶”к°Ђн•кі  мќґ StateлҐј м—…лЌ°мќґнЉё н•ґм•јм§Ђ л°”лЂЊлЉ” к°’мќ„ Stateм•€м—ђм„њ мќґ setмњјлЎњ м—…лЌ°мќґнЉё н•ґм•ј н•њл‹¤, м–ґл–»кІЊ н•м§Ђ?
                    //к°„л‹Ён•њ л©”м„њл“њлҐј л§Њл“¤кІ л‹¤ onChangeм—ђ handleUsernameChangeлќјкі  н•кІ л‹¤, м—¬кё° л©”м„њл“њ, м¦‰ н•Ём€лҐј мѓќм„±н•кІ л‹¤ //5-1: ліЂм€ л°‘м—ђм„њ мћ‘м„±..
                    <input type="text" name="username" value={username} onChange={handleUsernameChange} />
                </div>
                <div>
                    <label>Password:</label>
                    <input type="password" name="password"/>
                </div>
                <div>
                    <button type="button" name="login">login</button>
                </div>
            </div>
        </div>
    )
}

function WelcomeComponent() {
    return (
        <div className='Welcome'>
            Welcome Component
        </div>
    )
}
