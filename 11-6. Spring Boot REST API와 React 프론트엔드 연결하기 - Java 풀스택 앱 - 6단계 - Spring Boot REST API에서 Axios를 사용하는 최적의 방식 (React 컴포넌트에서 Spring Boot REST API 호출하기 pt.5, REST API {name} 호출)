import {useParams, Link} from 'react-router-dom'
import { useState } from 'react'

//6: ì´ì œ ì´ retrieveHelloWorldBean ë©”ì„œë“œ, ì´ ì„í¬íŠ¸ê°€ ì‚¬ìš©ë˜ì§€ ì•Šê³  ê·¸ëŸ¼ ê·¸ê±¸ ì‚­ì œí•˜ê³  ì €ì¥í•´ë³´ê² ë‹¤ ({ retrieveHelloWorldBean, retrieveHelloWorldPathVariable }ì˜ retrieveHelloWorldBeanì‚­ì œ..)
import { retrieveHelloWorldPathVariable } from './api/HelloWorldApiService'

function WelcomeComponent() {

    const {username} = useParams()

    const [message, setMessage] = useState(null)

    function callHelloWorldRestApi() {
        console.log('called')

        //6-1: ê·¸ë¦¬ê³  ì—¬ê¸°ì— ì½”ë©˜íŠ¸ë¡œ ëœ ì½”ë“œê°€ ì¢€ ìˆëŠ”ë° ì´ê²ƒë„ ì‚­ì œí•´ë³´ì, ì €ì¥í•˜ê³ 

        /*        (<== ì—¬ê¸°ì„œë§Œ ì£¼ì„ ì²˜ë¦¬ ì‹¤ì œ ì½”ë“œëŠ” ì‚­ì œ)
        // axios.get('http://localhost:8080/hello-world')   
        //     .then( (response) => successfulResponse(response) )
        //     .catch( (error) => errorResponse(error) )
        //     .finally( () => console.log('cleanup') )
        */

        /*        (<== ì—¬ê¸°ì„œë§Œ ì£¼ì„ ì²˜ë¦¬ ì‹¤ì œ ì½”ë“œëŠ” ì‚­ì œ)
        // retrieveHelloWorldBean()
        //     .then( (response) => successfulResponse(response) )
        //     .catch( (error) => errorResponse(error) )
        //     .finally( () => console.log('cleanup') )        
        */

        retrieveHelloWorldPathVariable('Ranga')  
            .then( (response) => successfulResponse(response) )
            .catch( (error) => errorResponse(error) )
            .finally( () => console.log('cleanup') )          
    }

