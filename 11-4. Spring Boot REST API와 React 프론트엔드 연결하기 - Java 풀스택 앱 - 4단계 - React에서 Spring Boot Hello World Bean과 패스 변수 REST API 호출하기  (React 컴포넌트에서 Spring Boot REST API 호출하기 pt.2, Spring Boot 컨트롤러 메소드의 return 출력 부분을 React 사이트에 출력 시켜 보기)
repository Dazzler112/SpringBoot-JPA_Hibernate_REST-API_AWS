import {useParams, Link} from 'react-router-dom'
import axios from 'axios'
//1 кі„м†Ќ: useStateк°Ђ мћ„нЏ¬нЉёлђЁ.. 
import { useState } from 'react'

function WelcomeComponent() {

    const {username} = useParams()

    //1: к·ё мѓЃнѓњм—ђ л©”м‹њм§ЂлҐј л„Јмќ„ кІѓмќґл‹¤, к·ёлџј мќґл ‡кІЊ useState()лҐј нём¶њн•  к±°кі  кё°ліёк°’мќЂ (null)мќј кІѓмќґл‹¤
    //мќґл ‡кІЊ мµњмґ€ л©”м‹њм§ЂлЉ” nullмќј кІѓмќґкі , мќґл ‡кІЊ useStateлҐј л¦¬м•ЎнЉём—ђм„њ мћ„нЏ¬нЉён–€м§Ђ, к·ёлџ¬л©ґ мќґк±ґ 2к°њмќ м›ђм†ЊлЎњ лђњ л°°м—ґмќ„ л¦¬н„ґн•кІЊ лђм§Ђ, мљ°л¦°мќґ л¶Ђл¶„мќ„ м¶©л¶„нћ€ м—°мЉµмќ„ н–€м§Ђ?
    //л„€лЉ” [] м—¬кё° л­ м Ѓм–јм§Ђ м•„мЈј мћ м•Њ кІѓмќґл‹¤ к·ёлџј [message, setMessage]лќјкі  м Ѓкі  messageмќ мґ€кё°к°’мќЂ nullмќґ лђ  кІѓмќґл‹¤ //1-1: return м•€мњјлЎњ..
    const [message, setMessage] = useState(null)

    function callHelloWorldRestApi() {
        console.log('called')

        axios.get('http://localhost:8080/hello-world')
            .then( (response) => successfulResponse(response) )
            .catch( (error) => errorResponse(error) )
            .finally( () => console.log('cleanup') )
    }

    function successfulResponse(response) {
        console.log(response)
    }

    function errorResponse(error) {
        console.log(error)
    }

    return (
        <div className='WelcomeComponent'>
            <h1>Welcome {username}</h1>
            <div>
                Manage Your todos - <Link to="/todos">Go here</Link>
            </div>
            <div>
                <button className="btn btn-success m-5" onClick={callHelloWorldRestApi}>
                    Call Hello World</button>
            </div>
    
          //1-1: к·ёл¦¬кі  мљ°л¦° м—¬кё°м—ђ <div>лҐј л§Њл“¤ кІѓмќґл‹¤, к·ёлџј <div>лќјкі  н•кі  className="text-info"лќјкі  н•ґмЈјмћђ нЏ‰м†ЊлЊЂлЎњ м—¬кё° нЃ° л”°мґн‘њлҐј м‚¬мљ©н•  к±°кі 
          //к·ёл¦¬кі  м—¬кё°м—ђ {message}лќјкі  л„Јм–ґм¤„ кІѓмќґл‹¤
          <div className="text-info">{message}</div>
        </div>
    )
}

export default WelcomeComponent
