package com.in28minutes.rest.webservices.restfulwebservices.user;

import static org.springframework.hateoas.server.mvc.WebMvcLinkBuilder.*;

import java.net.*;
import java.util.*;

import org.springframework.hateoas.*;
import org.springframework.hateoas.server.mvc.*;
import org.springframework.http.*;
import org.springframework.web.bind.annotation.*;
import org.springframework.web.servlet.support.*;

import com.in28minutes.rest.webservices.restfulwebservices.jpa.*;

import jakarta.validation.*;

@RestController
public class UserJpaResource {
	
	private UserRepository repository;

  //2: ê²Œì‹œë¬¼ ì €ì¥ì†Œë¥¼ í™œìš©í•´ì„œ ì‚¬ìš©ì ìƒì„±ë„ ê°€ëŠ¥í•˜ê²Œ í•˜ë ¤ê³  í•˜ëŠ”ë° PostRepositoryë¥¼ ì…ë ¥í•˜ê³  ì´ê±¸ postRepositoryë¼ê³  í•˜ê² ë‹¤
  //importë¬¸ì„ ì •ë¦¬í•˜ê³  ì¢‹ë‹¤ ì—¬ê¸°ì— ìƒì„±ì ì£¼ì…ë„ ì‚¬ìš©í•˜ë ¤ í•œë‹¤
  private PostRepository postRepository;

  //2-1: (UserRepository repository)ì— ì‰½í‘œë¥¼ ì°ê³  PostRepository postRepositoryë¥¼ ë„£ëŠ”ë‹¤
	public UserJpaResource(UserRepository repository, PostRepository postRepository) {
		this.repository = repository;

    //2-1 ê³„ì†: ì´ì œ this.postRepository = postRepository ë¼ê³  ì…ë ¥í•œë‹¤, ì €ì¥í•œë‹¤, ì´ì œ ê²Œì‹œë¬¼ì„ ìƒì„±í•  ë©”ì„œë“œë¥¼ ìƒì„±í•´ì•¼ê² ì§€ retrievePostsFouUser()ë©”ì†Œë“œë¥¼ ì‹œì‘ì ìœ¼ë¡œ ì‚¬ìš©í•˜ê² ë‹¤ ë³µì‚¬í•˜ê³  ë§¨ì•„ë˜ì— ë¶™ì—¬ë„£ê² ë‹¤ //2-2: ë§¨ì•„ë˜ë¡œ..
    this.postRepository = postRepository;
	}

	@GetMapping("/jpa/users")
	public List<User> retrieveAllUsers() {
		
		return repository.findAll();
	}
	
	//	http://localhost:8080/users
	
	//EntityModel
	//WebMvcLinkBuilder
	
	@GetMapping("/jpa/users/{id}")
	public EntityModel<User> retrieveUser(@PathVariable int id) {
		Optional<User> user = repository.findById(id);
		
		if(user.isEmpty())
			throw new UserNotFoundException("id:"+id);
		
		EntityModel<User> entityModel = EntityModel.of(user.get());
		
		WebMvcLinkBuilder link = linkTo(methodOn(this.getClass()).retrieveAllUsers());
		entityModel.add(link.withRel("all-users"));
		
		return entityModel;

	}

	@DeleteMapping("/jpa/users/{id}")
	public void deleteUser(@PathVariable int id) {
		repository.deleteById(id);
		
	}
	
	@GetMapping("/jpa/users/{id}/posts")
	public List<Post> retrievePostsForUser(@PathVariable int id) {
		Optional<User> user = repository.findById(id);
		
		if(user.isEmpty())
			throw new UserNotFoundException("id:"+id);
		
		return user.get().getPosts();
	}
	
	@PostMapping("/jpa/users")
	public ResponseEntity<User> createUser(@Valid @RequestBody User user) {
		User savedUser = repository.save(user);
		// /users/4 => /users /{id},  user.getID
		URI location = ServletUriComponentsBuilder.fromCurrentRequest()
						.path("/{id}")
						.buildAndExpand(savedUser.getId())
						.toUri();  
		
		return ResponseEntity.created(location).build();
	}

  //2-2: ì´ì œ createPostForUserë¼ê³  ì…ë ¥í•˜ê³  @PathVariableì— ì¶”ê°€ë¡œ ì…ë ¥í•  ë‚´ìš©ì€ createUser()ë©”ì†Œë“œì— ìˆëŠ” @Validë¡œ ì‹œì‘í•˜ëŠ” ë¶€ë¶„ì¸ë° ìš”ì²­ ë³¸ë¬¸ì„ ê²€ì¦í•˜ë ¤ëŠ” ê±°ë‹ˆê¹Œ @Validì™€ @RequestBodyë¥¼ ë„£ëŠ”ë‹¤
  //ì‚¬ìš©ìë¥¼ ê²Œì‹œë¬¼ì— ë§¤í•‘í•˜ë ¤ê³  í•˜ë‹ˆê¹Œ ì—¬ê¸´ @Valid @RequestBody Post postê°€ ë˜ì–´ì•¼ê² ì§€, ê·¸ëŸ¼ ì´ íŠ¹ì • ì‚¬ìš©ì IDì— ëŒ€í•´ ê²Œì‹œë¬¼ì„ ìƒì„±í•˜ê²Œ ëœë‹¤
  //ì›í•œë‹¤ë©´ ê²Œì‹œë¬¼ì— í•  ê²€ì¦ë„ ì •ì˜í•  ìˆ˜ ìˆë‹¤ //2-3:Post í´ë˜ìŠ¤ íŒŒì¼ë¡œ..
	@PostMapping("/jpa/users/{id}/posts")
  //2-12: List<Post> ì—ì„œ ë°˜í™˜íƒ€ì…ì´ ResponseEntity<Object>ë¡œ ë°”ë€œ, ëìœ¼ë¡œ í•´ì•¼ í•  ì¼ì´ í•˜ë‚˜ ë” ìˆë‹¤ ,ì˜¬ë°”ë¥¸ URLì„ ë§¤í•‘í•˜ëŠ” ê²ƒì´ë‹¤ @PostMappingìœ¼ë¡œ ë°”ê¿”ì¤˜ì•¼ê² ì§€? ê·¸ë¦¬ê³  importë¬¸ì„ ì •ë¦¬í•˜ê³  ì €ì¥..
	public ResponseEntity<Object> createPostForUser(@PathVariable int id, @Valid @RequestBody Post post) {

    //2-4: ê·¸ëŸ¬ë©´ ì‚¬ìš©ìë¥¼ ê²€ìƒ‰í•´ì„œ ì‚¬ìš©ìê°€ ë¹„ì–´ ìˆìœ¼ë©´ ì˜ˆì™¸ë¥¼ ë˜ì§€ê²Œ ëœë‹¤ 
		Optional<User> user = repository.findById(id);
		
		if(user.isEmpty())
			throw new UserNotFoundException("id:"+id);

    //2-5: ì´ì œ ì—¬ê¸°ì— ê²Œì‹œë¬¼ì— ëŒ€í•œ ì‚¬ìš©ìë¥¼ ì„¤ì •í•´ì•¼ê² ì§€? ì§€ê¸ˆ ì´ Postí´ë˜ìŠ¤ë¡œ ê°€ë³´ë©´ ì‚¬ìš©ìì— ëŒ€í•œ Gettersì™€ Settersê°€ ì—†ë‹¤, ë°”ë¡œ ìƒì„±í•´ë³´ì//2-6:Postí´ë˜ìŠ¤íŒŒì¼ë¡œ..
    //2-7: ì´ì œ ì—¬ê¸°ë¡œ ì™€ì„œ ì…ë ¥í•œë‹¤ post.setUser(user)ë¼ê³  í•˜ë©´ ê²Œì‹œë¬¼ì— ì‚¬ìš©ìë¥¼ ì„¤ì •í•˜ê²Œ ëœë‹¤, ì´ê±´ ì„ íƒì‚¬í•­ì¸ë°, ì—¬ê¸°ì— .getì„ ì…ë ¥í•´ì•¼ê² ë‹¤ user.getì´ ëœë‹¤, ì˜ëë‹¤ 
    post.setUser(user.get());

    //2-8: postRepositoryë¥¼ í™œìš©í•´ë³´ê² ë‹¤, ê²Œì‹œë¬¼ ì €ì¥ì†Œê°€ ìˆìœ¼ë‹ˆê¹Œ ê³§ë°”ë¡œ postRepository.saveë¼ê³  ì…ë ¥í•˜ê³ (post)ë¥¼ ë„£ì–´ì¤€ë‹¤, 
    //ê·¸ë¦¬ê³  ê²Œì‹œë¬¼ì„ ì €ì¥í–ˆìœ¼ë©´ ê·¸ì— ëŒ€í•œ URIë„ ë§Œë“¤ì–´ì•¼ê² ì§€? 
    //2-10: ì´ë ‡ê²Œ ì…ë ¥í•˜ê² ë‹¤ Post savedPost = postRepository ê·¸ëŸ¼ savedPost.getIdë¥¼ ì‚¬ìš©í•˜ê³  URLì„ ìƒì„±í•œ ë‹¤ìŒ ì´ê±¸ ë°˜í™˜ë°›ëŠ”ë‹¤ //2-11: ë‹¤ì‹œ ì•„ë˜ë¡œì§ìœ¼ë¡œ..
    Post savedPost = postRepository.save(post);

    //2-9: createUser()ë©”ì†Œë“œì™€ ê°™ì€ ê±¸ë¡œ ì‚¬ìš©í•´ë³´ê² ë‹¤ createUser()ì—ì„œ ì½”ë“œë¥¼ ë³µì‚¬í•œë‹¤, ì´ì œ fromCurrentRequest ìœ„ì¹˜ì— IDë¥¼ ë¶™ì—¬ì„œ ê²½ë¡œë¡œ ì‚¬ìš©í•˜ë ¤ê³  í•˜ëŠ”ë°,
    //ìš°ë¦¬ê°€ í•„ìš”í•œ ê±´ ì—¬ê¸°ì— ì…ë ¥ëœ savedUser.getID()ê°€ ì•„ë‹ˆë¼ ì €ì¥ëœ ê²Œì‹œë¬¼ì˜ ì„¸ë¶€ ì •ë³´, ì¦‰ ìƒì„±ëœ ê²Œì‹œë¬¼ì˜ IDë‹¤ //2-10: ë‹¤ì‹œ ìœ„ ë¡œì§ postRepository.save(post);ìœ¼ë¡œ
		//2-11: savedUser.getId()ë¥¼ ìœ„ì— ìƒì„±í•œ savedPost.getId()ë¡œ ì˜ ëœê²ƒê°™ë‹¤
    URI location = ServletUriComponentsBuilder.fromCurrentRequest()
						.path("/{id}")
						.buildAndExpand(savedPost.getId())
						.toUri();  

    //2-12: ì´ë¶€ë¶„ì— ì»´íŒŒì¼ì—ëŸ¬ê°€ ë‚˜ì˜¤ëŠ”ë° ctrl+1ë¡œ ë©”ì„œë“œ ë°˜í™˜ íƒ€ì…ì„ ResponseEntity<Object>ë¡œ ë°”ê¿”ì¤€ë‹¤ //2-12: ë©”ì†Œë“œëª…ìœ¼ë¡œ..
		return ResponseEntity.created(location).build();
	}
}
