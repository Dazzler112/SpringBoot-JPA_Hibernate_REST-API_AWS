export default function Counter() {
    return(
    <div className='Counter'>
    //3: мќґм њ Counterм•€м—ђ м№ґмљґнЉё м€«мћђлҐј н‘њм‹њн•кё° мњ„н•њ мљ”м†ЊлҐј м¶”к°Ђн•ґм•ј н•њл‹¤ к·ёлћм„њ 
    //div м•€м—ђ span мљ”м†ЊлҐј л„Јкі  нЃґлћмЉ¤ мќґл¦„мќ„ 'counter'лЎњ м§Ђм •н•њ л‹¤мќЊ, мќґ м•€м—ђм„њ м€«мћђлҐј н‘њм‹њн•  кІѓмќґл‹¤, мќјл‹Ё м§Ђкё€мќЂ 0мњјлЎњ н‘њм‹њн•кІ л‹¤
        <span className='count'>0</span>
    
    //3-1: л°‘м—ђлЉ” лІ„нЉјмќ„ м¶”к°Ђн•  кІѓмќґл‹¤, лІ„нЉјмќ нЃґлћмЉ¤ мќґл¦„мќЂ 'counterButton'мњјлЎњ н•кІ л‹¤ к·ёл¦¬кі  мќјл‹Ё м§Ђкё€ м¦ќк°Ђ(increment)лІ„нЉјл§Њ м¶”к°Ђн•кІ л‹¤, м–ґл–»кІЊ л‚м¤лЉ”м§Ђ ліґмћђ
        <button className="counterButton"></button>
    </div>
    )
}
