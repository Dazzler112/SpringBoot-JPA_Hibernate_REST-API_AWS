package com.example.demo.a0;

import java.util.*;

import org.springframework.context.annotation.*;

//3: ì´ë¦„ì„ ìƒˆë¡œ ì§€ì •í•´ë³´ì SimpleSpringContextLauncherApplication
//ë‹¤ìŒë¶€í„° ìƒˆ Spring ì»¨í…ìŠ¤íŠ¸ë¥¼ ì‹œì‘í•  ë•Œë§ˆë‹¤ ì´ê²ƒì„ ì˜ˆë¡œ ì‹œì‘í•  ìˆ˜ ìˆë‹¤ //ë‹¤ì‹œ DepInjectionLauncherApplicationí´ë˜ìŠ¤ íŒŒì¼ë¡œ...

@Configuration
@ComponentScan
public class SimpleSpringContextLauncherApplication {
	
	public static void main(String[] args) {

		try(var context = 
				new AnnotationConfigApplicationContext
				(SimpleSpringContextLauncherApplication.class)) {
			
			Arrays.stream(context.getBeanDefinitionNames()).forEach(System.out::println);

		}
	} 
}


