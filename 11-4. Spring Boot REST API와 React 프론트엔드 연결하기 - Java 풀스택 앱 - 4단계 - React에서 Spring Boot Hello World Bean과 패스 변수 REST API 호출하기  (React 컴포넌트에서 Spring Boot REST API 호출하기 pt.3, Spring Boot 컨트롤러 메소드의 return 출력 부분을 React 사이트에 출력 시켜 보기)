import { useState } from 'react'

function WelcomeComponent() {

    const {username} = useParams()

    //1: к·ё мѓЃнѓњм—ђ л©”м‹њм§ЂлҐј л„Јмќ„ кІѓмќґл‹¤, к·ёлџј мќґл ‡кІЊ useState()лҐј нём¶њн•  к±°кі  кё°ліёк°’мќЂ (null)мќј кІѓмќґл‹¤
    //мќґл ‡кІЊ мµњмґ€ л©”м‹њм§ЂлЉ” nullмќј кІѓмќґкі , мќґл ‡кІЊ useStateлҐј л¦¬м•ЎнЉём—ђм„њ мћ„нЏ¬нЉён–€м§Ђ, к·ёлџ¬л©ґ мќґк±ґ 2к°њмќ м›ђм†ЊлЎњ лђњ л°°м—ґмќ„ л¦¬н„ґн•кІЊ лђм§Ђ, мљ°л¦°мќґ л¶Ђл¶„мќ„ м¶©л¶„нћ€ м—°мЉµмќ„ н–€м§Ђ?
    //л„€лЉ” [] м—¬кё° л­ м Ѓм–јм§Ђ м•„мЈј мћ м•Њ кІѓмќґл‹¤ к·ёлџј [message, setMessage]лќјкі  м Ѓкі  messageмќ мґ€кё°к°’мќЂ nullмќґ лђ  кІѓмќґл‹¤ //1-1: return м•€мњјлЎњ..
