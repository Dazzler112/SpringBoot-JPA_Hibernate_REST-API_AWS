//8-1: к·ёлџј useParamsм—ђ м¶”к°ЂлЎњ useNavigate лќјкі  м Ѓм–ґліґмћђ ( {useParams} л¶Ђл¶„м—ђ , useNavigate м¶”к°Ђ..) к·ёлџј мљ°л¦° мќґлЏ™н•  м€ мћ€л‹¤ /todosлЎњ мќґлЏ™н•  м€ мћ€м§Ђ //8-2: updateTodoApi()лЎњ..
import {useNavigate, useParams} from 'react-router-dom'
import { retrieveTodoApi, updateTodoApi} from './api/TodoApiService'
import { useAuth } from './security/AuthContext'
import { useEffect, useState } from 'react'
import { Formik, Form, Field, ErrorMessage } from 'formik'


export default function TodoComponent() {

    const {id} = useParams()

    const[description, setDescription] = useState('')

    const[targetDate, setTargetDate] = useState('')

    const authContext = useAuth()

    //8: useAuth() л°”лЎњ м•„лћм—ђ useNavigate()лҐј л„Јмќ„ к±°кі , ctrl+spaceлҐј л€Њлџ¬м„њ к·ёл¦¬кі  react-router-domм—ђм„њ useNavigateлҐј мћ„нЏ¬нЉён•њл‹¤ //8-1: мћ„нЏ¬нЉёлЎњ..
    const navigate = useNavigate()

    const username = authContext.username

    useEffect(
        () => retrieveTodos(),
        [id]
        )

    function retrieveTodos() {
        retrieveTodoApi(username, id)
        .then(response => {
            setDescription(response.data.description)
            setTargetDate(response.data.targetDate)
        })
        .catch(error => console.log(error))
    }

    function onSubmit(values) {
        console.log(values)
        const todo = {
            id: id,
            username: username,
            description: description,
            targetDate: targetDate,
            done: false
        }
        console.log(todo)

        updateTodoApi(username, id, todo)
        .then(response => {
            //8-2: к·ёлџј м„±кіµм Ѓмќё мќ‘л‹µмќ„ л°›мњјл©ґ, мљ°л¦¬лЉ” navigate()лќјкі  н•кі  мћ‘мќЂ л”°мґн‘њ м•€м—ђ '/todos'лќјкі  м Ѓм–ґм¤Ђл‹¤, мќґк±ё м ЂмћҐн•кі 
            navigate('/todos')
        })
        .catch(error => console.log(error))
    }

    function validate(values) {
        let errors = {
            // description: 'Enter a valid description',
            // targetDate: 'Enter a valid targetDate'
        }

        if(values.description.length < 5) {
            errors.description = 'Enter atleast 5 characters'
        }    

        if(values.targetDate.length == null) {
            errors.targetDate = 'Enter a target date'
        }         

        console.log(values)
        return errors
    }

...
}
