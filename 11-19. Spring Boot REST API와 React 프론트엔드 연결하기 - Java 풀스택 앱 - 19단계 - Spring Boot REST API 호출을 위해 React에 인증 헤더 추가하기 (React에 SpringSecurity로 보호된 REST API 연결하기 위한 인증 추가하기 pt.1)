        baseURL: 'http://localhost:8080'
    }
)

export const retrieveHelloWorldBean 
        = () => apiClient.get('/hello-world-bean')

export const retrieveHelloWorldPathVariable 
          //1: к·ёлџј м—¬кё° мќґ л©”м„њл“њм—ђ л„Њ мќґл ‡кІЊ н•ґм¤„ м€ мћ€л‹¤, м¦‰ к°ќмІґлЎњ л§Њл“¤кі  
  = (username) => apiClient.get(`/hello-world/path-variable/${username}`, {
    //1-1: мќґ к°ќмІґмќ мќјл¶ЂлЎњм„њ л„€лЉ” AuthorizationмќґлќјлЉ” к±ё л„Јм–ґм¤„ м€ мћ€л‹¤ к·ёлџј headers:{}лќјкі  н•кі 
    headers:{
      //1-2: Authorizationмќґлќјкі  н•ґм¤„ м€ мћ€м§Ђ, к·ёл¦¬кі  мљ°л¦¬к°Ђ м „м†Ўн•л ¤лЉ” мќём¦ќ н—¤лЌ”лЉ” л­ђм§Ђ?
      Authorization:
    }
  })
