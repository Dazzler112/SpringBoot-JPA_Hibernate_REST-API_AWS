⏺ App.js



import './App.css';
import Counter from './components/counter/Counter';

function App() {
  return (
     <div className="App">
       <Counter />
       <Counter />           
       <Counter />
     <PlayingWithProps />
    </div>
  )
}

function PlayingWithProps() {
  return(

    //3: 안에 'Props'라는 텍스트를 넣어서 화면에 보이게 만들겠다
    <div>Props</div>
  )
}

export default App;
