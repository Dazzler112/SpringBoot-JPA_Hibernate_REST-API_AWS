import './App.css';
import Counter from './components/counter/Counter';

function App() {
  return (
    <div className="App">
    //1: лЁјм Ђ App.js нЊЊмќјлЎњ к°Ђм„њ CounterлҐј ліµм‚¬н•ґ '<Counter />', мќґл ‡кІЊ л¶™мќґл©ґ лђкІ м§Ђ?
     <Counter />
     <Counter />
     <Counter />
    </div>
  )
}

export default App;

