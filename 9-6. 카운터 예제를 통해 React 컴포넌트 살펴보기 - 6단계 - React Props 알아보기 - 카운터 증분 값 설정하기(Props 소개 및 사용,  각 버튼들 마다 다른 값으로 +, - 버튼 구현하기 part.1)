//7-1: мќґлІ€м—ђлЉ” лЄЁлЌ мћђл°”мЉ¤нЃ¬л¦ЅнЉё л°©м‹ќмњјлЎњ н”„лЎњнЌјн‹° к°’м—ђ м ‘к·јн•лЉ” к±ё ліґм—¬мЈјкІ л‹¤
//л‹¤лҐё м‚¬лћЊл“¤мќґ мќґлџ° м‹ќмњјлЎњ м‚¬мљ©н•лЉ” к±ё ліё м Ѓмќґ мћ€мќ„ кІѓмќґл‹¤, лЁјм Ђ м¤‘кґ„нёлҐј м—ґкі  л‹«мќЂ л‹¤мќЊ
//к·ё м•€м—ђ property1 кіј property2лҐј л„Јмњјл©ґ лђњл‹¤
function PlayingWithProps({property1, property2}) {

  //7-2: мќґл ‡кІЊ н•л©ґ console.log()м—ђм„њ property1кіј property2лҐј л°”лЎњ м‚¬мљ©н•  м€ мћ€м§Ђ, л¦¬м•ЎнЉём—ђм„њ н”„лЎњнЌјн‹°лҐј м‚¬мљ©н•  л•ЊлЉ” ліґн†µ мќґлџ° м‹ќмњјлЎњ мЅ”л“њлҐј мћ‘м„±н•њл‹¤
  console.log(property1)
  console.log(property2)
  return(
    <div>Props</div>
  )
}

export default App;

