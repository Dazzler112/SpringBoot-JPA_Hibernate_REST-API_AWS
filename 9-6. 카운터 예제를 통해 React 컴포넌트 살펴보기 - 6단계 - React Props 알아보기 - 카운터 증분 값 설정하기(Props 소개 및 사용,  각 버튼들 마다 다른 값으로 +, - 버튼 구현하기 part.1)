import './App.css';
import Counter from './components/counter/Counter';

function App() {
  return (
    <div className="App">

    //5: мќґ м»ґнЏ¬л„ЊнЉём—ђ property1="value1" property2="value2"лҐј м¶”к°Ђн•л©ґ, н•Ём€н• м»ґнЏ¬л„ЊнЉё м•€м—ђм„њ мќґ к°’м—ђ м ‘к·јн•  м€ мћ€л‹¤
    //к°’м—ђ м ‘к·јн•лЉ” л°©лІ•мќЂ н•Ём€м—ђ л§¤к°њліЂм€лҐј м¶”к°Ђн•лЉ” кІѓмќґл‹¤// 5-1: PlayingWithProps() н•Ём€лЎњ..
     <PlayingWithProps />
     <Counter />
     <Counter />           
     <Counter />
    </div>
  )
}

//5-1: н•Ём€м—ђ 'properties'лќјлЉ” л§¤к°њліЂм€лҐј м¶”к°Ђн•ґ ліґкІ л‹¤, н•Ём€м—ђ л°н™н•кё° м „ к°„л‹Ён•њ лЎњк·ёлҐј н•л‚ м¶њл Ґн•кІ л‹¤
function PlayingWithProps(properties) {
    //5-2: console.log()лЎњ propertiesлҐј м¶њл Ґн•кІ л‹¤, м ЂмћҐн•њ л‹¤мќЊ 
    console.log(properties)

  return(
    <div>Props</div>
  )
}

export default App;
