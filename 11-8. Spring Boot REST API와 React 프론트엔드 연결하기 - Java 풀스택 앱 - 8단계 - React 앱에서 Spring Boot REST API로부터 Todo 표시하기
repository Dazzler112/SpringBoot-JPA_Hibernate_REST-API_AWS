вЏє ListTodosComponent.jsx




import { useState } from "react"

function ListTodosComponent() {

    const today = new Date()

    const targetDate = new Date(today.getFullYear() + 12, today.getMonth(), today.getDay())

    const [todos, setTodos] = useState([])

    // const todos = [
    //     {id : 1, description : 'Learn AWS', done : false, targetDate : targetDate},
    //     {id : 2, description : 'Learn Full Stack Dev', done : false, targetDate : targetDate},
    //     {id : 3, description : 'Learn DevOps', done : false, targetDate : targetDate}
    //   ]

    //6: к·ёлџј .then()мќґлќјкі  н•кі  мќ‘л‹µмќ„ лЎњк№…н•  к±°кі , к·ёлџј (response => console.log(response))лќјкі  н•ґм¤Ђл‹¤
    //к·ёлџј мќ‘л‹µмќґ м„±кіµм Ѓмќґл©ґ мљ°л¦¬лЉ” мќ‘л‹µмќ„ лЎњк№…н•кі  
    retrieveAllTodosForUsername('in28minutes')
    .then(response => console.log(response))

    //6-1: к·ёл ‡м§Ђ м•ЉмќЂ кІЅмљ°м—ђлЏ„ catch()лќјкі  н•ґмЈјкі , (error => console.log(error))лќјкі  н•ґм¤Ђл‹¤ 
    .catch(error => console.log(error))

    return (
        <div className='container'>
            <h1>Things You Want To Do!</h1>
            <div>
                <table className='table'>
                    <thead>
                        <tr>
                            <td>ID</td>
                            <td>Description</td>
                            <td>is Done?</td>
                            <td>Target Date</td>
                        </tr>
                    </thead>
                    <tbody>
                        {
                            todos.map(
                                todo => (
                                    <tr key={todo.id}>
                                        <td>{todo.id}</td>
                                        <td>{todo.description}</td>
                                        <td>{todo.done.toString()}</td>
                                        <td>{todo.targetDate.toDateString()}</td>
                                   </tr>
                                )
                            )
                        }
                    </tbody>
                </table>
            </div>
        </div>
    )
}

export default ListTodosComponent
