package com.firstapplication.springboot.myfirstwebapp.login;

import org.slf4j.*;
import org.springframework.stereotype.*;
import org.springframework.ui.*;
import org.springframework.web.bind.annotation.*;

@Controller
public class LoginController {

  //1: ì—¬ê¸° Looggerë¥¼ ì´ˆê¸°í™”í•  ê²ƒì´ê³ , ê·¸ëŸ¼ private Logger logger = ë¼ê³  í•˜ê³  ì´ê±´ íŒ©í† ë¦¬ íŒ¨í„´ì´ë¼ëŠ” ê±¸ ì‚¬ìš©í•œë‹¤ ê·¸ëŸ¼ LoggerFactory.getLogger()ë¼ê³  í•˜ê³  getLogger()ì•ˆì— getClass()ë¥¼ ë„£ì–´ì£¼ì
	//ê·¸ëŸ¼ LoggerFactoryê°€ ì´ë¯¸ ì„í¬íŠ¸ëœ ê±¸ ë³¼ ìˆ˜ ìˆë‹¤, org.slf4j.LoggerFactoryê°€ ì„í¬íŠ¸ë˜ì—ˆë‹¤ ì˜¬ë°”ë¥¸ Loggerë¥¼ ì„í¬íŠ¸ í•´ì•¼í•œë‹¤ ìš°ë¦¬ê°€ ì‚¬ìš©í•˜ëŠ” LoggerëŠ” slfjì´ë‹¤ 
  //ê·¸ëŸ¼ ì´ì œ ë””ë²„ê¹…ì„ í•´ ë³¼ê¹Œ? ë­”ê°€ë¥¼ ë¡œê¹…í•´ë³´ì ë©”ì„œë“œ ì½”ë“œë¡œ ê°€ë³´ì//2:gotLoginPage ë©”ì„œë“œë¡œ..
  private Logger logger = LoggerFactory.getLogger(getClass());
	

  //http://localhost:8087/login?name=Ranga
	//Model
	@RequestMapping("login")
	public String gotLoginPage(@RequestParam String name, ModelMap model) {
		model.put("name", name);
    
    //2: logger.debug()ë¼ê³  í•  ìˆ˜ ìˆë‹¤ ê·¸ë¦¬ê³  ìš°ë¦° nameì„ ë””ë²„ê·¸í•˜ë ¤ í•œë‹¤, ê·¸ëŸ¬ë©´ "Request param is" ë¼ê³  í•˜ê³ , ê·¸ê±¸ ì²¨ë¶€í•˜ëŠ” ëŒ€ì‹ ì— ë„ˆëŠ” ì¤‘ê´„í˜¸ë¥¼ ì—´ê³  ë‹«ì„ ìˆ˜ ìˆë‹¤ ì´ê²Œ nameìœ¼ë¡œ ëŒ€ì²´ë  ê²ƒì´ë‹¤ ê·¸ë¦¬ê³  ì´ê±¸ ì €ì¥ ê·¸ë¦¬ê³  URLë¡œ ë“¤ì–´ê°€ë³´ì
    logger.debug("Request param is {}", name);

		System.out.println("Request Param is " + name); //NOT RECOMENDED FOR PROD CODE
		return "login";
	}
}

