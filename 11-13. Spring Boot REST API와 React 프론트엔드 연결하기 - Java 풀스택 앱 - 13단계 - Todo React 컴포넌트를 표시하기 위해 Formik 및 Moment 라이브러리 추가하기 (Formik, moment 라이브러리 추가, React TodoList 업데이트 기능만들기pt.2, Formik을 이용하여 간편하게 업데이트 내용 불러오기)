вЏє TodoComponent.jsx




import {useParams} from 'react-router-dom'
import { retrieveTodoApi } from './api/TodoApiService'
import { useAuth } from './security/AuthContext'
import { useEffect, useState } from 'react'

//1-1: import { Formik } from 'formik' мќґлќјкі  н•ґ м¤„ кІѓмќґл‹¤ 
import { Formik } from 'formik'

export default function TodoComponent() {

    const {id} = useParams()

    const[description, setDescription] = useState('')

    const authContext = useAuth()

    const username = authContext.username

    useEffect(
        () => retrieveTodos(),
        [id]
        )

    function retrieveTodos() {
        retrieveTodoApi(username, id)
        .then(response => {
            setDescription(response.data.description)
        })
        .catch(error => console.log(error))
    }

    return (
        <div className="container">
            <h1>Enter Todo Details</h1>
            <div>
                //1: м—¬кё° м•€мњјлЎњ м™Ђм„њ м—¬кё° мћ€лЉ” descrition:{description} лҐј м‚­м њн•кі  м–‘м‹ќмќ„ м¶”к°Ђн•ґліґмћђ 
                //к·ёлџј <Formik>нѓњк·ёлҐј л„Јкі  л‹«лЉ” нѓњк·ёлҐј л„Јм–ґліґкІ л‹¤ мќґл ‡кІЊ <Formik>нѓњк·ёлҐј л„Јм—€кі  к·ёкІЊ м–ґл””м„њ м¤лЉ” к±ёк№Њ? //1-1: importлЎњ..
                <Formik>
                    
                </Formik>
            </div>
        </div>
    ) 
}
