import {useParams} from 'react-router-dom'
import { retrieveTodoApi } from './api/TodoApiService'
import { useAuth } from './security/AuthContext'
import { useEffect, useState } from 'react'

//3-1: ( {}ì— ,Form ì‘ì„± ) ê·¸ëŸ¼ ì„í¬íŠ¸ë¥¼ í•´ë³´ì 
//3-5: ê·¸ë¦¬ê³  ì—­ì‹œ Fieldë„ formikì—ì„œ ì˜¨ë‹¤ ({}ì— Field ì¶”ê°€..)
import { Formik, Form, Field } from 'formik'


export default function TodoComponent() {

    const {id} = useParams()

    const[description, setDescription] = useState('')

    const authContext = useAuth()

    const username = authContext.username

    useEffect(
        () => retrieveTodos(),
        [id]
        )

    function retrieveTodos() {
        retrieveTodoApi(username, id)
        .then(response => {
            setDescription(response.data.description)
        })
        .catch(error => console.log(error))
    }

    return (
        <div className="container">
            <h1>Enter Todo Details</h1>
            <div>
                <Formik>
                    {
                    (props) =>  (
                        //3: <Form> ì´ë¼ê³  ì ëŠ”ë‹¤, ì—­ì‹œ ì´ê²ƒë„ formikì—ì„œ ì˜¤ê³ //3-1: importë¡œ.. 
                        <Form>
                        //3-2: ê·¸ë¦¬ê³  <Form>ì•ˆì— ëª¨ë“  ìš”ì†Œê°€ ë“¤ì–´ê°ˆ ê²ƒì´ë‹¤, í†µìƒì ìœ¼ë¡œ <Form>ì•ˆì˜ ëª¨ë“  ìš”ì†ŒëŠ” í•„ë“œ ì„¸íŠ¸ ì•ˆì— í¬í•¨ëœë‹¤, ê·¸ëŸ¼ í•„ë“œì„¸íŠ¸ë¥¼ ë§Œë“¤ì–´ ë³´ì
                          <fieldset>
                            //3-3: ëª¨ë“  í•„ë“œì—ëŠ” ë ˆì´ë¸”ì´ ìˆì„ ê±°ê³  ê·¸ëŸ¼ <label>ì„ ë„£ì–´ì£¼ì
                            <label></label>
                            //3-4: ê·¸ë¦¬ê³  ë³´í†µì€ ì…ë ¥ê°’ì´ ìˆì§€, í•˜ì§€ë§Œ ì–‘ì‹ì—ì„  í•„ë“œë¼ëŠ” ê±¸ ë§Œë“¤ì–´ì•¼ í•œë‹¤ ê·¸ëŸ¼ ì—¬ê¸°ì— <Field>ë¥¼ ë§Œë“ ë‹¤ //3-5: ë‹¤ì‹œ importë¡œ..
