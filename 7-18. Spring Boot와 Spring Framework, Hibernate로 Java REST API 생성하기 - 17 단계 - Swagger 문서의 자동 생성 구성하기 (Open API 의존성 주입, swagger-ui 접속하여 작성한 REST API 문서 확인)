  "version": "v0"
  },
    "servers": [
      {
        "url": "http://localhost:8080",
        "description": "Generated server url"
      }
    ],
        "paths": {
        "/users": {
        "get": {
        "tags": [
        "user-resource"
      ],
        "operationId": "retrieveAllUsers",
        "responses": {
        "200": {
        "description": "OK",
        "content": {
        "*/*": {
        "schema": {
        "type": "array",
        "items": {
        "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        }
      },
