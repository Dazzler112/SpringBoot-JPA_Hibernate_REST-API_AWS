      "content": {
      "application/json": {
      "schema": {
      "$ref": "#/components/schemas/User"
            }
          }
        },
      "required": true
      },
      "responses": {
      "200": {
      "description": "OK",
      "content": {
      "*/*": {
      "schema": {
      "$ref": "#/components/schemas/User"
                  }
                }
              }
            }
          }
        }
