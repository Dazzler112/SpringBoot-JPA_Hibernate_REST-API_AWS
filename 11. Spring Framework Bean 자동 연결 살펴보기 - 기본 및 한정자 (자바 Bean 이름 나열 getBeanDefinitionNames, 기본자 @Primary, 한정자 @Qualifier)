package com.example.demo;

import org.springframework.beans.factory.annotation.*;
import org.springframework.context.annotation.*;


record Person (String name, int age, Address address) { };


//Address - firstLine & city
record Address(String firstLine ,String city) { };

@Configuration 
public class HelloWorldConfiguration {
	
	
	@Bean
	public String name () {
		
		return "Ranga";
	}
	
	@Bean
	public int age () {
		
		return 20;
	}

	@Bean
	public Person person() {
		return new Person("Ravi", 25, new Address("Main Street","Utrecht"));
	}
	
	@Bean
	public Person person2MethodCall() {
		return new Person(name(), age(), address()); //name, age

	}
	
	@Bean
	public Person person3Parameters(String name, int age, Address address3) {
		//name, age, address2
		return new Person(name, age, address3); //name, age

	}
	//ì—¬ê¸°ì—ì„œ person3Parametersê°€ ì„¤ì •ë˜ë©° person3ParametersëŠ” í˜„ì œ address3 Beanì„ ì‚¬ìš©í•œë‹¤ Bean ì´ë¦„ì€ ì—¬ê¸°ì—ì„œ ì‚¬ìš©í•˜ê³  ìˆë‹¤ (@Bean(name = "address3")) (Address address3)
	//ë‹¤ë¥¸ Beanì„ ë§Œë“¤ì–´ ì£¼ê² ë‹¤
	
	
	//person4Parametersë¥¼ ë§Œë“¤ê³  address3ëŒ€ì‹  addressë¥¼ ì…ë ¥í•œë‹¤ ì‚¬ì‹¤ ì´ë¦„ì´ addressì¸ Beanì€ ì—†ë‹¤ ì´ë¦„ì´ address2ì¸ Beanê³¼ address3ì¸ Beanì´ ìˆë‹¤ í•˜ì§€ë§Œ ì´ë¦„ì´ addressì¸ Beanì€ ì—†ë‹¤
	//ì´ëŸ¬ë©´ ì–´ë–»ê²Œ ë ê¹Œ? í™•ì¸í•´ë³´ì ìë°” ì‹¤í–‰í•´ë³´ê¸°
	@Bean
	@Primary
	//No qualifying bean of type 'com.example.demo.Address' 
	//available: expected single matching bean but found 2: address2,address3
	public Person person4Parameters(String name, int age, Address address) {
		//name, age, address2
		return new Person(name, age, address); //name, age

	}
	/* ì¶œë ¥ê²°ê³¼ ==>
	No qualifying bean of type 'com.example.demo.Address' available: expected single matching bean but found 2: address2,address3
	ë™ì¼í•œ ì˜¤ë¥˜ê°€ í‘œì‹œë˜ê³  ê³ ìœ í•œ Bean ì •ì˜ ì˜ˆì™¸ëŠ” ì—†ë‹¤ ê·¸ë¦¬ê³  ìœ í˜•ì´ Addressì¸ ìê²©ì„ ì¶©ì¡±í•˜ëŠ” Beanì„ ì´ìš©í•  ìˆ˜ ì—†ë‹¤ê³  ì´ì™€ ê°™ì€ ì˜¤ë¥˜ê°€ í‘œì‹œëœë‹¤
	ì¼ì¹˜í•˜ëŠ” ë‹¨ì¼ Beanì´ ìˆì–´ì•¼ í•˜ì§€ë§Œ address2ì™€ addres3ì˜ ë‘ ê°œê°€ ìˆë‹¤ //App02HelloWorldSpring3 í´ë˜ìŠ¤ íŒŒì¼ë¡œ..
	*/
	//(@Primary ì ì–´ì£¼ê¸°) ê°€ì ¸ì˜¤ê¸°ë¥¼ êµ¬ì„±í•˜ê² ë‹¤ ê·¸ë¦¬ê³  ì‹¤í–‰í•´ë³´ì //App02HelloWorldSpring3í´ë˜ìŠ¤ì˜ (Person.class)ë¡œ..
	
	
	//address3ì˜ @Qualifierë¥¼ ì‚¬ìš©í•˜ë ¤ê³  í•˜ë‹ˆ @Qualifier("address3qulifier")ì„ ë³µì‚¬í•´ì„œ íŒŒë¼ë¯¸í„°ì˜ Address addressì— ë„£ì–´ ì£¼ê² ë‹¤
	//ì´ ë¶€ë¶„ì´ addressì— ìë™ìœ¼ë¡œ ì—°ê²°ë˜ê³  nameì´ ì—¬ê¸°ì— ì˜¤ëŠ” ìƒˆ personì„ ë§Œë“¤ê² ë‹¤(return new Person(name, age, address)) ê·¸ë¦¬ê³  person í•œì •ìì˜ ê°’ì„ ê°€ì ¸ì˜¤ê² ë‹¤ 
	//ê·¸ëŸ¬ë©´ ë§ˆì§€ë§‰ ê²ƒì„ ì¶œë ¥í•´ ë³´ê² ë‹¤ //ë‹¤ì‹œ App02HelloWorldSpring3í´ë˜ìŠ¤íŒŒì¼ ë¡œ.. 
	@Bean
	public Person person5Qulifier(String name, int age, @Qualifier("address3qulifier") Address address) {
		//name, age, address2
		return new Person(name, age, address); //name, age

	}
	
ğŸŸ¢ @Primary	
	//ì´ë ‡ê²Œ ì…ë ¥í•˜ê³˜ë‹¤ @Primary ë°°ì¹˜ë¥¼ ì¡°ì§í•˜ê³  ì´ë ‡ê²Œ address ë¬¸ì œë¥¼ í•´ê²°í–ˆë‹¤ ì´ì œ ì• í”Œë¦¬ì¼€ì´ì…˜ì„ ë‹¤ì‹œ ì‹¤í–‰í•´ë³´ë©´//App02HelloWorldSpring3í´ë˜ìŠ¤ì˜ (Address.class)ë¡œ..
	@Bean(name = "address2")
	@Primary
	public Address address () {
		return new Address("Baker Street","London");
	}
 
