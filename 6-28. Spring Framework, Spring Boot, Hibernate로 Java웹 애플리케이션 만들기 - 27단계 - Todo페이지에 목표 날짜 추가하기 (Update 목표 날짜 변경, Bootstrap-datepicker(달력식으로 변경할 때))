вЏє todo.jsp нЊЊмќј



<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %>

<html>
	<head>
		<link href="webjars/bootstrap/5.1.3/css/bootstrap.min.css" rel="stylesheet">
		<title>
			Add Todo Page
		</title>
	</head>
	<body>
			<!-- <div>Welcome to in28minutes</div> -->
			<div class="container">
			
				<h1>Enter Todo Details</h1>
				<form:form method="post" modelAttribute="todo">

					<fieldset class="mb-3">
						<form:label path="description">Description</form:label>
						<form:input type="text" path="description" required="required"/>
						<form:errors path="description" cssClass="text-warning"/>				
					</fieldset>	

          <!--3: к·ёлџј мњ„м—ђк±ё ліµм‚¬н•ґ м—¬кё°м—ђ л¶™м—¬ л„ЈлЉ”л‹¤, к·ёл¦¬кі  мќґк±ґ "targetDate"м—ђ кґЂн•њ кІѓмќґл‹¤-->
          <!--3-1: мќґ targetDateлЉ” Todo Beanм—ђ мћ€лЉ” кІѓкіј м •н™•нћ€ мќјм№н•ґм•ј н•л‹€ кј­ н™•мќён•кё° л°”лћЂл‹¤ (Todo.javaмќ tagetDateм™Ђ н•„л“њк°Ђ мќјм№н•лЉ”м§Ђ н™•мќён›„ мЌЁлќј!)-->
          <!--3-2: мљ°л¦¬лЉ” Todo BeanмњјлЎњл¶Ђн„° н•„л“њлҐј <form>м—ђ л§¤н•‘н•кі  мћ€л‹¤ к·ёлџј inputлЏ„ path="targetDate"кі , errorsлЏ„ path="targetDate"л‹¤, мќґл ‡кІЊ л„€к°Ђ targetDateлҐј л„Јм–ґм•ј н•лЉ” кіімќґ м„ё кіімќґл‹¤-->
					<fieldset class="mb-3">
            <!--3-3: к·ёл¦¬кі  л„€к°Ђ м—¬кё°м—ђлЏ„ Description м—ђм„њ Target DateлЎњ л°”кї” л„Јм–ґм¤м•ј н•њл‹¤, мќґл ‡кІЊ н•„л“њмќ м‹¤м њ л €мќґлё”лЏ„ м—­м‹њ Target Dateк°Ђ лђм–ґм•ј н•њл‹¤ мќґм њ м ЂмћҐн•кі  List Todos Page м‚¬мќґнЉёлЎњ лЏЊм•„к°Ђмћђ..-->
						<form:label path="targetDate">Target Date</form:label>
						<form:input type="text" path="targetDate" required="required"/>
						<form:errors path="targetDate" cssClass="text-warning"/>				
					</fieldset>	            
									
					<form:input type="hidden" path="id"/>
					<form:input type="hidden" path="done"/>
					<input type="submit" class="btn btn-success"/>
				
				</form:form>
				
			</div>
			<script type="webjars/bootstrap/5.1.3/js/bootstrap.min.js"></script>
			<script type="webjars/jquery/3.6.0/jquery.min.js"></script>
	
	</body>
</html>

