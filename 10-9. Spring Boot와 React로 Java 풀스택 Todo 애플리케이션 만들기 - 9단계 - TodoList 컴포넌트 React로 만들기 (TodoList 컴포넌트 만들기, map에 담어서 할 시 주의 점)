        </div>
    )
}

function ListTodosComponent() {

    //3: м—¬кё°м—ђ constлЎњ м •мќн•кІ л‹¤ todos = мќјл‹Ё н•л‚лЎњ н•ґліјк№Њ todosмќ к°’мњјлЎњ idк°Ђ 1, к·ёл¦¬кі  descriptionмќ„ мќґл ‡кІЊ мЌЁліґмћђ 'Learn AWS'
    const todos = {id : 1, description : 'Learn AWS'}

    return (
        <div className='ListTodosComponent'>
            <h1>Things You Want To Do!</h1>
      
            //3-1: к·ёл¦¬кі  м—¬кё°м—ђм„њ Todo DetailsлҐј ліґм—¬мЈјлЉ” к±°м§Ђ, мќјл‹ЁмќЂ todosк°Ђ н•л‚мќёлЌ° л‚м¤‘м—ђлЉ” л§Ћм•„м§€ кІѓмќґл‹¤, к·ёлћм„њ м—¬кё°м—ђ н…Њмќґлё”мќ„ л§Њл“¤кІ л‹¤
            <div>
                //3-2: Todos лЄ©лЎќмќ„ ліґм—¬мЈјкё° мњ„н•њ н…Њмќґлё”мќ„ л§Њл“¤кі  н…Њмќґлё” нѓњк·ёлЏ„ л‹«м•„м¤Ђл‹¤
                <table>
                //3-3: к·ёл¦¬кі  н…Њмќґлё”м—ђлЉ” н•­мѓЃ theadлќјкі  н—¤лЌ”к°Ђ л“¤м–ґк°ЂлЉ” кіімќґ мћ€л‹¤, н…Њмќґлё” н—¤лЌ”лЉ” м—¬кё° theadм—ђ л“¤м–ґк°Ђм•јн•њл‹¤
                     <thead>
                       //3-5: theadм•€м—ђ trмќЂ н–‰мќ„ л§ђн•њл‹¤
                        <tr>
                           //3-6: мќјл‹Ё мљ°л¦¬лЉ” idл§Њ мћ€мњјл‹€к№Њ <td>id к·ёл¦¬кі  descriptionмќґм§Ђ ,к°„л‹Ён•њ н…Њмќґлё”мќ„ л§Њл“¤м–ґлґ¤л‹¤, м—¬кё° trмќ„ ліµм‚¬н•ґм„њ tbodyм—ђлЏ„ л„Јм–ґмЈјкІ л‹¤
                           <td>id</td>
                           <td>description</td>
                        </tr>
                    </thead>
                //3-4: к·ёл¦¬кі  tbodyлЉ” м‹¤м њ мЅн…ђмё к°Ђ л“¤м–ґк°ЂлЉ” к±°м§Ђ, 
                    <tbody>
                      //3-7: (л¶™м—¬л„Јкё°) мќјл‹Ё м ЂмћҐн•кі  л“¤м—¬м“°кё° н•њл‹¤
                        <tr>
                            //3-8: к·ёл¦¬кі  м—¬кё°лЉ” idм™Ђ description лЊЂм‹  todosмќ к°’мќ„ ліґм—¬мЈјкі  м‹¶л‹¤
                            //к·ёлџ¬л ¤л©ґ todos.idм™Ђ todos.descriptionмќґлќјкі  м Ѓмњјл©ґ лђкІ м§Ђ, JavaScriptм—ђм„њ к°’мќ„ к°Ђм ём™Ђ JSXм—ђм„њ ліґм—¬мЈјлЉ” кІѓмќґл‹¤, м¤‘кґ„нё м‚¬мќґм—ђ л„Јм–ґмЈјм—€л‹¤, м ЂмћҐн•кі 
                            <td>{todos.id}</td>
                            <td>{todos.description}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    )
}
