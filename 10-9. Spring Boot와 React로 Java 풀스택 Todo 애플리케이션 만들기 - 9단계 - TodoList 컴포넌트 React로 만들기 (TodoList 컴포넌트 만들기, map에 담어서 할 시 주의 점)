function ListTodosComponent() {

    //4: м—¬кё°м—ђм„њ л°°м—ґмќ„ л§Њл“¤ кІѓмќґл‹¤, {}мќґк±ґ н•њ к°њ мљ”м†ЊлҐј к°Ђм§„ л°°м—ґмќґкі  мљ”м†ЊлҐј лЌ” л§Њл“¤м–ґ ліґкІ л‹¤
    //м—¬кё°м—ђ мљ”м†ЊлҐј л‹ґлЉ” кІѓмќґл‹¤ мІмќЊк»Ђ 1, Learn AWSм§Ђ л‘ђ лІ€м§ёлЉ” м§Ђкё€ н•кі  мћ€лЉ” к±ё м ЃкІ л‹¤, Learn Full Stack Dev, м„ё лІ€м§ёлЉ” Learn DevOpsлќјкі  н•кІ л‹¤ Kubernetesл‚ DockerлЏ„ л°°мљ°кі  м‹¶л‹¤л©ґ DevOpsлҐј л°°м›Њм•јкІ м§Ђ
    //к·ёл¦¬кі  м—¬кё°м—ђ л‹¤ ліґм—¬мЈјкі  м‹¶л‹¤, м—¬кё° мћ€лЉ” м„ё к°њмќ к°’мќ„ к°Ѓк°Ѓмќ н–‰мњјлЎњ м—¬кё° ліґм—¬мЈјкі  м‹¶мќЂ к±°м§Ђ, к·ё м „м—ђ мќјл‹Ё м—¬кё°лЎњ к°Ђм„њ JavaScript мЅм†”лЎњ к°€ кІѓмќґл‹¤
    const todos = [
                    {id : 1, description : 'Learn AWS'},
                    {id : 1, description : 'Learn Full Stack Dev'},
                    {id : 1, description : 'Learn DevOps'}
                  ]

    return (
        <div className='ListTodosComponent'>
            <h1>Things You Want To Do!</h1>
            <div>
                <table>
                    <thead>
                        <tr>
                            <td>id</td>
                            <td>description</td>
                        </tr>
                    </thead>
                    <tbody>
                         <tr>
                         <td>{todos.id}</td>
                         <td>{todos.description}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </div>
    )
}
