package com.in28minutes.rest.webservices.restfulwebservices.basic;


@Configuration
public class BasicAuthenticationSecurityConfiguration {

	//Filter chain
	// authenticate all request
	//basic authentication
	//disabling csrf
	//stateless rest api
	
	@Bean                                                //5кі„м†Ќ..: (м—¬кё° throws Excetionмќґ мѓќк№Ђ...)
	public SecurityFilterChain filterChain(HttpSecurity http) throws Exception {

    //5: м—¬кё°м„њ ctrl+1мќ„ л€„лҐґл©ґ Add throws declarationмќ„ м„ нѓќн•  м€ мћ€кі  м—¬кё°м—ђ м»ґнЊЊмќј м¤лҐк°Ђ л‚лЉ” мќґмњ лЉ” http.build()к°Ђ м€м™ёлҐј л‚ґкё° л•Њл¬ёмќґл‹¤
    //к·ёлџј л„€лЉ” Add throws declarationмќ„ м„ нѓќн•ґмЈјмћђ
		return http.build();
	}
}

