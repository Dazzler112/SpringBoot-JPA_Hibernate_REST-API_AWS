import {BrowserRouter, Routes, Route} from 'react-router-dom'
import './TodoApp.css'
import LogoutComponent from './LogoutComponent'
import HeaderComponent from './HeaderComponent'
import ListTodosComponent from './ListTodosComponent'
import ErrorComponent from './ErrorComponent'
import WelcomeComponent from './WelcomeComponent'
import LoginComponent from './LoginComponent'
import AuthProvider from './security/AuthContext'


            //2-3: м—¬кё° childrenмќґ мћ€м–ґм•ј кІ м§Ђ, л‹¤лҐё м»ґнЏ¬л„ЊнЉёлҐј л§ђн•њл‹¤
function AuthencatedRoute({children}) {

    //2-4: мќјл‹Ё к·ёлѓҐ childrenмќ„ л°н™н•ґ ліґкІ л‹¤, м ЂмћҐн•л©ґ м–ґл–»кІЊ лђм§Ђ?
    return children
}

export default function TodoApp() {
    return(
        <div className='TodoApp'>
            <AuthProvider>
            <BrowserRouter>
                <HeaderComponent />
                <Routes>
                    <Route path='/' element={<LoginComponent />} />
                    <Route path='/login' element={<LoginComponent />} />

              //2: мќём¦ќмќґ н•„мљ”н•њ лќјмљ°н„°лЉ” м—¬кё°м—ђ мќґл ‡кІЊ л„Јм–ґмЈјкі 
                    <Route path='/welcome/:username' element={
                  //2-1:  <AuthencatedRoute>м™Ђ л‹«лЉ” нѓњк·ё м‚¬мќґм—ђ л„Јм–ґм¤Ђл‹¤
                    <AuthencatedRoute/>
                      <WelcomeComponent />
                    //2-2: л‹«м•„ м¤м•ј н•њл‹¤, мќґлџ°м‹ќмњјлЎњ н•  м€ мћ€л‹¤, м›°м»ґ м»ґнЏ¬л„ЊнЉёлЉ” мќём¦ќ лќјмљ°н„°мќ мћђм‹ќмќґ лђкі , м›°м»ґ м»ґнЏ¬л„ЊнЉёлЉ” м‚¬мљ©мћђк°Ђ мќём¦ќлђм—€мќ„ л•Њл§Њ ліґмќґкІЊ лђ  кІѓмќґл‹¤ //2-3: л‹¤м‹њ AuthencatedRoute() н•Ём€лЎњ..
